					EXP:ARG:펠라티오경험 ++
			CASE 2
				TCVAR:PLAYER:4 += 150
				SOURCE:ARG:봉사 += 200
				SOURCE:ARG:쾌M += 200
			;딥스로트
			CASE 3
				TCVAR:PLAYER:4 += 500
				SOURCE:ARG:봉사 += 300
				SOURCE:ARG:고통 += 100
				SOURCE:ARG:쾌M += 300
			;펠라 자위
			CASE 4
				SOURCE:ARG:쾌C += 100
				SOURCE:ARG:노출 += 200
				SOURCE:ARG:쾌M += 200
				EXP:ARG:자위경험 ++
		ENDSELECT
	;파이즈리
	CASE 504
		SELECTCASE TCVAR:ARG:21
			CASE 1
				LOCALS = {ARG}/
				SOURCE:PLAYER:쾌C += 400
				CSTR:PLAYER:10 += LOCALS
				TCVAR:PLAYER:2 = 5
				;강도
				TCVAR:PLAYER:4 += 300
				SIF !EXP_UP(26, ARG)
					EXP:ARG:파이즈리경험 ++
			;세로 파이즈리
			CASE 2
				TCVAR:PLAYER:4 += 300
				SOURCE:ARG:유혹 += 300
				SOURCE:ARG:봉사 += 300
			;골짜기를 만든다
			CASE 3
				TCVAR:PLAYER:4 += 500
				SOURCE:ARG:봉사 += 500
			CASE 4
				TCVAR:PLAYER:4 += 1000
				SOURCE:ARG:봉사 += 200
				SOURCE:ARG:사랑 += 100
				SOURCE:ARG:달성 += 100
				SOURCE:ARG:쾌M += 200
				EXP:ARG:펠라티오경험 ++
				EXP:ARG:파이즈리경험 ++
				;애정경험
				SIF TALENT:ARG:연모 && ASSIPLAY == 0
					EXP:ARG:애정경험 += 1
				;노예가 고양이혀 소유의 경우는, 조교자에게 가학쾌락경험을 추가
				SIF TALENT:ARG:고양이혀
					EXP:ARG:가학쾌락경험 ++
		ENDSELECT
	;가랑이
	CASE 505
		SELECTCASE TCVAR:ARG:21
			CASE 1
				LOCALS = {ARG}/
				SOURCE:PLAYER:쾌C += 400
				CSTR:PLAYER:10 += LOCALS
				TCVAR:PLAYER:2 = 6
				;강도
				TCVAR:PLAYER:4 += 300
			;수음 추가
			CASE 2
				TCVAR:PLAYER:4 += 300
				SOURCE:ARG:봉사 += 300
				EXP:ARG:수음경험 ++
		ENDSELECT
	;억지로 질성교
	CASE 506
		SELECTCASE TCVAR:ARG:21
			;몸을 비튼다
			CASE 1
				SOURCE:ARG:고통 += 300
				SOURCE:ARG:반감 += 2000
				DOWNBASE:ARG:체력 += 50
				DOWNBASE:ARG:기력 += 100
			;눈물
			CASE 2
				SOURCE:ARG:굴종 += 100
				SOURCE:ARG:반감 += 1000
				DOWNBASE:ARG:기력 += 50
		ENDSELECT
	;구슬 비비어
	CASE 600
		TCVAR:PLAYER:4 += 200
		SOURCE:ARG:유혹 += 200
		SOURCE:ARG:봉사 += 200
		SOURCE:ARG:도발 += 200
		SOURCE:ARG:정복 += 300
		SIF TALENT:ARG:새드 || ABL:ARG:새드끼 > 4
			SOURCE:ARG:가학 += 100
			TCVAR:ARG:기학플래그 = 1
	;애널애무
	CASE 601
		TCVAR:PLAYER:4 += 100
		SOURCE:ARG:송구스러운 += 300
		SOURCE:ARG:가학 += 100
		SOURCE:PLAYER:쾌A += 300
		TCVAR:ARG:기학플래그 = 1
ENDSELECT

IF TEQUIP:MASTER:V섹스 >= 0 || TEQUIP:MASTER:A섹스 >= 0
	IF TEQUIP:ARG:가슴주무르기
		TEQUIP:ARG:가슴주무르기 = 0
		PRINTL ＜가슴주무르기 해제＞
	ENDIF
	IF TEQUIP:ARG:유두빨기
		TEQUIP:ARG:유두빨기 = 0
		PRINTL ＜유두빨기 해제＞
	ENDIF
ENDIF

CALL 마스터카운터제어(ARG)

@DATUI_FOR_INSERT(ARG)
IF BEDROOM(CFLAG:MASTER:현재위치)
	SIF TEQUIP:ARG:0
		CALL DATUI_BOTTOM_T(ARG,0)
ELSE
	SIF TEQUIP:ARG:0 & 14
		CALL DATUI_BOTTOM_T(ARG,1)
