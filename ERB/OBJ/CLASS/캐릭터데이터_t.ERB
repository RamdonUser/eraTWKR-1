@EXIST_캐릭터데이터_ID
#LOCALSIZE 1
#LOCALSSIZE 1

;통칭으로부터
;요소명→ID변환용 함수
;필요하면 각 오브젝트로와 같이  작성
@캐릭터데이터_ID(ARG:0, ARGS:0, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS V_NAME                  ;작성하는 변수명
#DIMS C_NAME, OBJ_ID_LAST + 1 ;데이터 보유
#DIM  O_ID                    ;참조용
IF ARGS:0 == "RESET"
	VARSET C_NAME
	FOR O_ID, 1, OBJ_ID_LAST + 1
		C_NAME:O_ID = %GET_STR(, "캐릭터데이터", O_ID, "통칭")%
	NEXT
ELSE
	O_ID = FINDELEMENT(C_NAME, ESCAPE(ARGS:0), , , 1)
	CALLF MAKE_INT(V_NAME, O_ID)
ENDIF



;존부 함수
;F함수는 TRYCALL계를 사용할 수 없기 때문에 통상 함수로 존부 배열을 만들어버린다
@EXIST_캐릭터데이터1
#LOCALSIZE 1
#LOCALSSIZE 1

;오브젝트 본체
@캐릭터데이터1(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "레이무")

;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(속옷) 세트")

CASE "평상복：액세서리"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("액세서리", "소매"))
CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "리본"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "무릎까지 오는 양말"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
	ENDIF
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "하쿠레이의 무녀옷"))

CASE "고유커맨드"
	CALLF MAKE_STR(V_NAME, "운세뽑기계약")
CASE "고유커맨드 실행 가능"
	CALLF MAKE_INT(V_NAME, !FLAG:운세뽑기계약)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "디저트/검소/상품/일식/양식/고기 요리/쌀/야채/과일/팥소/단/담백/귀여운/장난/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야취/금단/" * 3)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "영험/행운/")

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "작은 붉은 리본이 프런트를 뒤따르고 있다")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "프런트 리본 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "리본 브래지어")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 틈이 날 수 있다"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;농민 관련
;신사는 토지 가져 그렇게, 칸다
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "쌀 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "쌀")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "논")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "하쿠레이 신사")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 400)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 5)

;여기는 의뢰 ID가 관련하지 않는 정보
;의뢰 발생율 스카2/일반3/고유 5
CASE "의뢰 없음 발생율"
	CALLF MAKE_INT(V_NAME, 2)
CASE "일반의뢰발생율"
	CALLF MAKE_INT(V_NAME, 3)
CASE "고유의뢰발생율"
	CALLF MAKE_INT(V_NAME, 5)
CASE "고유의뢰최종 요소"
	CALLF MAKE_INT(V_NAME, 103)
CASEELSE
	;고유의뢰관련의 데이터에 바톤 터치
	SIF STRCOUNT(O_DATA, "의뢰")
		CALLF 고유의뢰1(ARG, O_DATA, V_NAME)
ENDSELECT



