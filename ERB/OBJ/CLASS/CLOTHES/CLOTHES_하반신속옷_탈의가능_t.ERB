	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신속옷1", "마이크로 탑"))
ENDSELECT

@승부속옷판정(ARG)
#FUNCTION
IF !TALENT:ARG:무지
	SIF (CFLAG:ARG:기정사실 & 합의_우후후 || TALENT:ARG:연모) && CFLAG:ARG:다음날내방
		RETURNF 1
	IF DAY:2 == 1
		SELECTCASE DAY:3
			CASE 14
				SIF TALENT:ARG:연모
					RETURNF 1
		ENDSELECT
	ELSEIF DAY:2 == 4
		SELECTCASE DAY:3
			CASE 14, 24, 25
				SIF TALENT:ARG:연모
					RETURNF 1
		ENDSELECT
	ENDIF
ENDIF
RETURNF 0

