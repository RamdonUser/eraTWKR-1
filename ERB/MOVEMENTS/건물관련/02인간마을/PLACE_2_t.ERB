@MAP_UNIQUEMESSAGE_2
CALL DRAW_INFORMATIONLINE("Info", 1)
IF WITH_MOB()
	RETURN 1
ELSE
	SELECTCASE CFLAG:MASTER:현재위치
		CASE 205
			IF INRANGE(TIME, 540, 1020)
				PRINTFORML 보기에도 의욕의 없을 것 같은 캇파가 색도의 접수를 하고 있다
			ELSE
				PRINTFORML 삭도역은 닫히고 있다
			ENDIF
		CASE 213
			PRINTFORML 점내에는 많은 책장이 줄지어 있다
		CASE 222
			IF TIME >= 480 && TIME <= 720 || TIME >= 780 && TIME <= 840 && FLAG: 70 != 1
				PRINTDATA
					DATAFORM 서당의 아이들은 %CALLNAME:TARGET%의 수업을 받고 있다…
				ENDDATA
				PRINTL
			ELSEIF TIME >= 720 && TIME <= 780 && FLAG:70 != 1
				PRINTDATA
					DATAFORM 서당의 아이들은 %타겟과% 식사하고 있다…
				ENDDATA
				PRINTL
			ELSEIF TIME >= 480 && TIME <= 840 && FLAG:70 == 1 && !CFLAG:MASTER:우후후
				PRINTDATA
					DATAFORM 서당의 아이들은 정지하고 있다…
				ENDDATA
				PRINTL
			ELSEIF TIME >= 480 && TIME <= 840 && FLAG:70 == 1 && CFLAG:MASTER:우후후
				PRINTDATA
					DATAFORM %타겟은% 서당의 아이들의 앞에서 습격당하고 있다…
				ENDDATA
				PRINTL
			ENDIF
		CASE 203 TO 206
			SIF !RAND:50 && INRANGE(TIME, 540, 1020)
				PRINTFORML 약 주머니를 가진 수염면의 남자가, 통행인에게 무엇인가 열심히 말하고 있다
	ENDSELECT
ENDIF
RETURN 1


