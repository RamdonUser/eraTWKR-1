
;지점 관련성 응답 처리
;-----------------------------------------------------------
@CAN_MOVE_2(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	;마을의 문
	;미나미토리와 직통
	CASE 201
		SIF ARG:1 == 203
			RETURNF 3
	;중앙 광장
	;미나미토리, 히가시도리, 기타도리, 니시도리, 용신 상과 직통 꽃집, 식료품점, 구경노에 이동가능
	CASE 202
		SIF ARG:1 == 203
			RETURNF 3
		SIF ARG:1 == 204
			RETURNF 3
		SIF ARG:1 == 205
			RETURNF 3
		SIF ARG:1 == 206
			RETURNF 3
		SIF ARG:1 == 207
			RETURNF 3
		SIF ARG:1 == 215
			RETURNF 1
		SIF ARG:1 == 216
			RETURNF 1
		SIF ARG:1 == 220
			RETURNF 1
	;미나미토리
	;마을의 문, 중앙 광장에 직통 선술집, 카페, 주택가, 식료품점, 일품 요리점, 목욕탕에 이동가능
	CASE 203
		SIF ARG:1 == 201
			RETURNF 3
		SIF ARG:1 == 202
			RETURNF 3
		SIF ARG:1 == 211
			RETURNF 1
		SIF ARG:1 == 212
			RETURNF 1
		SIF ARG:1 == 214
			RETURNF 1
		SIF ARG:1 == 216
			RETURNF 1
		SIF ARG:1 == 218
			RETURNF 1
		SIF ARG:1 == 223
			RETURNF 1
	;히가시도리
	;중앙 광장에 직통 코스즈이오리, 서당, 감미소, 집회소, 목욕탕에 이동가능
	CASE 204
		SIF ARG:1 == 202
			RETURNF 3
		SIF ARG:1 == 213
			RETURNF 1
		SIF ARG:1 == 217
			RETURNF 1
		SIF ARG:1 == 219
			RETURNF 1
		SIF ARG:1 == 222
			RETURNF 1
		SIF ARG:1 == 223
			RETURNF 1
		SIF ARG:1 == 224
			RETURNF 1
	;기타도리
	;중앙 광장, 용신 상에 직통 집회소, 서당, 아큐의 저택에 이동가능
	CASE 205
		SIF ARG:1 == 202
			RETURNF 3
		SIF ARG:1 == 207
			RETURNF 3
		SIF ARG:1 == 219
			RETURNF 1
		SIF ARG:1 == 221
			RETURNF 1
		SIF ARG:1 == 222
			RETURNF 1
	;니시도리
	;중앙 광장에 직통 주택가, 꽃집, 구경노, 아큐의 저택, 혼숙여관에 이동가능
	CASE 206
		SIF ARG:1 == 202
			RETURNF 3
		SIF ARG:1 == 214
			RETURNF 1
		SIF ARG:1 == 215
			RETURNF 1
		SIF ARG:1 == 220
			RETURNF 1
		SIF ARG:1 == 221
			RETURNF 1
		SIF ARG:1 == 225
			RETURNF 1
	;용신 상
	;중앙 광장, 기타도리와 직통
	CASE 207
		SIF ARG:1 == 202
			RETURNF 3
		SIF ARG:1 == 205
			RETURNF 3
	;선술집
	;미나미토리에 이동가능
	CASE 211
		SIF ARG:1 == 203
			RETURNF 1
	;카페
	;미나미토리에 이동가능
	CASE 212
		SIF ARG:1 == 203
			RETURNF 1
	;코스즈이오리
	;히가시도리에 이동가능
	CASE 213
		SIF GROUPMATCH(ARG:1, 204,223,226)
			RETURNF 1
	;주택가
	;니시도리에 이동가능
	CASE 214
		SIF ARG:1 == 206
			RETURNF 1
		SIF GROUPMATCH(ARG:1, 201,230,231,232,233,234,235,236,237)
			RETURNF 1
	;꽃집
	;중앙 광장, 니시도리에 이동가능
	CASE 215
		SIF ARG:1 == 202
			RETURNF 1
		SIF ARG:1 == 206
			RETURNF 1
	;식료품점
	;중앙 광장, 미나미토리에 이동가능
	CASE 216
		SIF ARG:1 == 202
			RETURNF 1
		SIF ARG:1 == 203
			RETURNF 1
	;감미소
	;히가시도리에 이동가능
	CASE 217
		SIF ARG:1 == 204
			RETURNF 1
	;일품 요리점
	;미나미토리에 이동가능
	CASE 218
		SIF ARG:1 == 203
			RETURNF 1
	;집회소
	;히가시도리, 기타도리, 서당에 이동가능
	CASE 219
		SIF ARG:1 == 204
			RETURNF 1
		SIF ARG:1 == 205
			RETURNF 1
		SIF ARG:1 == 222
			RETURNF 1
	;구경노
	;중앙 광장, 니시도리와 직통 마을의 문, 미나미토리, 히가시도리, 기타도리, 용신 상이 보인다
	CASE 220
		SIF ARG:1 == 201
			RETURNF 2
		SIF ARG:1 == 202
			RETURNF 3
		SIF ARG:1 == 203
			RETURNF 2
		SIF ARG:1 == 204
			RETURNF 2
		SIF ARG:1 == 205
			RETURNF 2
		SIF ARG:1 == 206
			RETURNF 3
		SIF ARG:1 == 207
			RETURNF 2
	;아큐의 저택
	;기타도리, 니시도리에 이동가능
	CASE 221
		SIF ARG:1 == 205
			RETURNF 1
		SIF ARG:1 == 206
			RETURNF 1
	;서당,
	;히가시도리, 기타도리, 집회소, -군요 하우스에 이동가능
	CASE 222
		SIF GROUPMATCH(ARG:1, 204,205,224)
			RETURNF 1
	;목욕탕
	;미나미토리, 히가시도리, 코스즈이오리에 이동가능
	CASE 223
		SIF ARG:1 == 203
			RETURNF 1
		SIF ARG:1 == 204
			RETURNF 1
		SIF ARG:1 == 213
			RETURNF 1
	;-군요 하우스
	;히가시도리, 기타도리, 집회소, 서당에 이동가능
	CASE 224
		SIF GROUPMATCH(ARG:1, 204,205,222)
			RETURNF 1
	;혼숙여관
	;니시도리에 이동가능
	CASE 225
		SIF ARG:1 == 206
			RETURNF 1
	;코스즈룸
	CASE 226
		SIF ARG:1 == 213
			RETURNF 1
	;연립 주택의 각부가게
	CASE 230 TO 235
		SIF GROUPMATCH(ARG:1, 214,230,231,232,233,234,235,236,237)
			RETURNF 1
	;연립 주택의 우물 화장실
	CASE 236,237
		SIF GROUPMATCH(ARG:1, 214,230,231,232,233,234,235,236,237)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == 201
			RETURNF 1
ENDSELECT
RETURNF 0