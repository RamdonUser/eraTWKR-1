;--------------------------------------------------
;	맵내 이동 처리 확장 패치 부속
;	명련사MAP용 함수
;--------------------------------------------------
@MAP_PLACENAME_8(ARG)
#FUNCTIONS

IF GET_MAPID(ARG) == MAIN_MAP
	SELECTCASE ARG
		CASE 1 TO (MAXROOM - 1)
			RETURNF STR:(ARG + 8000)
		CASE 898
			RETURNF OMANEKI_PLACE()
		CASE 899
			RETURNF "스키마 공간"
		CASE MAXROOM
			RETURNF "도의 저 편"
		CASEELSE
			RETURNF "불명"
	ENDSELECT
ELSE
	RETURNF "불명"
ENDIF

@MAP_VIEWING_8(AA)
#DIMS REF AA, 0
IF !TFLAG:맵변환
	SELECTCASE CFLAG:MASTER:현재위치
		CASE 830, 832, 833
			TFLAG:맵변환 = 3
		CASE 811 TO 826
			TFLAG:맵변환 = 2
		CASE 831, 834 TO 842
			TFLAG:맵변환 = 4
		CASEELSE
			TFLAG:맵변환 = 1
	ENDSELECT
ENDIF
CALL DRAW_INFORMATIONLINE(@"%MAPNAME:(TFLAG:맵변환)%")
PRINTFORML 
TRYCALLFORM MAP_SUMMIT_{TFLAG:맵변환}(AA)

@MAP_CAN_MOVE_8(ARG, ARG:1)
#FUNCTION
RETURNF 1


@MAP_CANNOT_WORD_8(ARG)
SELECTCASE TFLAG:이동불능메시지
ENDSELECT
