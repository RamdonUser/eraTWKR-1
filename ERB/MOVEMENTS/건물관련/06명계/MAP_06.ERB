@DRAW_MAP_BG_6(SLICED)
#DIMS SLICED
SELECTCASE SLICED
;色つけたいときはこんな感じ
CASE "木","林"
	SETCOLOR 0x90EE90
	PRINTS SLICED
	RESETCOLOR
;違う文字を表示させる例
CASE "#W"
	SETCOLOR 0x90EE90
	PRINT 木
	RESETCOLOR
CASE "#E"
	SIF TALENT:MASTER:위치짐작 && COUNTCHARA:(MAXROOM) > 0 && FLAG:짐작모드 == 1
		SETCOLOR 255,100,200
	PRINT ↓
	RESETCOLOR
;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT