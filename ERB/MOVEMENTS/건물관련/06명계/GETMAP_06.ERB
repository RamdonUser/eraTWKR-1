@GETMAP_6(ARG)
#DIM 기색
기색 = 0
IF TALENT:MASTER:위치짐작 && FLAG:짐작모드 == 1
	SIF ROOMDATA:(ARG % 100) & 장소_모브
		SETCOLOR C_D_PINK
	FOR LOCAL, 1, CHARANUM
		IF CFLAG:LOCAL:현재위치 == ARG
			IF CFLAG:LOCAL:행동 == 5
				기색 = 2
				BREAK
			ELSEIF CFLAG:LOCAL:수면
				기색 = 3
				BREAK
			ENDIF
			기색 = 1
		ENDIF
		IF ARG == 603 && !GROUPMATCH(CFLAG:MASTER:현재위치, 603,610,611,612,613,614)
			IF GROUPMATCH(CFLAG:LOCAL:현재위치, 603,610,611,612,613,614)
				IF CFLAG:LOCAL:행동 == 5
					기색 = 2
					BREAK
				ELSEIF CFLAG:LOCAL:수면
					기색 = 3
					BREAK
				ENDIF
				기색 = 1
			ENDIF
		ELSEIF ARG == 605 && !GROUPMATCH(CFLAG:MASTER:현재위치, 605,618)
			IF GROUPMATCH(CFLAG:LOCAL:현재위치, 605,618)
				IF CFLAG:LOCAL:행동 == 5
					기색 = 2
					BREAK
				ELSEIF CFLAG:LOCAL:수면
					기색 = 3
					BREAK
				ENDIF
				기색 = 1
			ENDIF
		ELSEIF ARG == 606 && !GROUPMATCH(CFLAG:MASTER:현재위치, 606,615,616,617)
			IF GROUPMATCH(CFLAG:LOCAL:현재위치, 606,615,616,617)
				IF CFLAG:LOCAL:행동 == 5
					기색 = 2
					BREAK
				ELSEIF CFLAG:LOCAL:수면
					기색 = 3
					BREAK
				ENDIF
				기색 = 1
			ENDIF
		ELSEIF ARG == 609 && !GROUPMATCH(CFLAG:MASTER:현재위치,619,620,621,622,623,624)
			IF GROUPMATCH(CFLAG:LOCAL:현재위치,619,620,621,622,623,624)
				IF CFLAG:LOCAL:행동 == 5
					기색 = 2
					BREAK
				ELSEIF CFLAG:LOCAL:수면
					기색 = 3
					BREAK
				ENDIF
				기색 = 1
			ENDIF
		ENDIF
		IF ARG == 603
			CALL GETMAP_BUILDING(LOCAL,610,614)
		ELSEIF ARG == 607
			CALL GETMAP_BUILDING(LOCAL,615,618)
		ELSEIF ARG == 608 || 609
			CALL GETMAP_BUILDING(LOCAL,619,624)
		ENDIF
		SIF 기색 > 1
			BREAK
	NEXT
	IF 기색 == 1
		SETCOLOR 255, 100, 200
	ELSEIF 기색 == 2
		SETCOLOR 255, 100, 0
	ELSEIF 기색 == 3
		SETCOLOR 0, 120, 180
	ENDIF
ENDIF
CALL GETMAP_YOGORE(ARG)
