
;지점 관련성 응답 처리
;-----------------------------------------------------------
@CAN_MOVE_14(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE MAXROOM
		SIF ARG:1 == 1401
			RETURNF 1
	CASE 1401
		SIF GROUPMATCH(ARG:1, 1413)
			RETURNF 1
		SIF ARG:1 == MAXROOM
			RETURNF 2
	CASE 1402
		SIF GROUPMATCH(ARG:1, 1409)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, 1413)
			RETURNF 1
	CASE 1403
		SIF GROUPMATCH(ARG:1, 1413,1404,1405)
			RETURNF 1
	CASE 1404
		SIF GROUPMATCH(ARG:1, 1413,1403)
			RETURNF 1
	CASE 1405
		SIF GROUPMATCH(ARG:1, 1413,1403,1406)
			RETURNF 1
	CASE 1406
		SIF GROUPMATCH(ARG:1, 1413,1405)
			RETURNF 1
	CASE 1407
		SIF GROUPMATCH(ARG:1, 1413,1408)
			RETURNF 1
	CASE 1408
		SIF GROUPMATCH(ARG:1, 1407)
			RETURNF 1
	CASE 1409
		SIF GROUPMATCH(ARG:1, 1402)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, 1410,1411)
			RETURNF 1
	CASE 1410
		SIF GROUPMATCH(ARG:1, 1409,1412)
			RETURNF 1
	CASE 1411
		SIF GROUPMATCH(ARG:1, 1409,1412)
			RETURNF 1
	CASE 1412
		SIF GROUPMATCH(ARG:1, 1410,1411)
			RETURNF 1
	CASE 1413
		SIF GROUPMATCH(ARG:1,1423,1443)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, 1401,1402,1403,1404,1405,1406,1407)
			RETURNF 1
	CASE 1414 TO 1422
		SIF GROUPMATCH(ARG:1, 1423)
			RETURNF 1
	CASE 1423
		SIF GROUPMATCH(ARG:1,1413,1433)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, 1414,1415,1416,1417,1418,1419,1420,1421,1422)
			RETURNF 1
	CASE 1424 TO 1432
		SIF GROUPMATCH(ARG:1, 1433)
			RETURNF 1
	CASE 1433
		SIF GROUPMATCH(ARG:1,1423)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, 1424,1425,1426,1427,1428,1429,1430,1431,1432)
			RETURNF 1
	CASE 1434 TO 1441
		SIF GROUPMATCH(ARG:1, 1443)
			RETURNF 1
	CASE 1442
		SIF GROUPMATCH(ARG:1, 1443,1444)
			RETURNF 1
	CASE 1443
		SIF GROUPMATCH(ARG:1,1413)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, 1434,1435,1436,1437,1438,1439,1440,1441,1442)
			RETURNF 1
	CASE 1444
		SIF GROUPMATCH(ARG:1, 1442)
			RETURNF 1
ENDSELECT
RETURNF 0