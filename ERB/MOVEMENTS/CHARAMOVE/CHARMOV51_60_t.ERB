;캐릭터별_각종 데이터
;51~60번까지의 캐릭터
;CALL		USER
;ARG		0:호출 데이터 NO, 1:이동 경향 취득시의 방NO
;RETURN		캐릭터별 호출 데이터치
;COMMENT	
;-----------------------------------------------------------
;니토리
@CHARAMOVE_DATA_51(ARG, ARG:2)
LOCAL = 51
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;늪에 자주(잘) 있다
			CASE 703
				RETURN 5
			;액가, 치유리 집, 슈우카, 카센가, 은둔지, 오두막
			CASE 706, 709, 715 TO 719, 720 TO 730, 733, 734
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 71,72,73,76, 81,82,83,84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

