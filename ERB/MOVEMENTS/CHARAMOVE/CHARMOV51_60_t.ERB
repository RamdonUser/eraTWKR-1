			;영원정내
			CASE 401, 402, 403, 404, 405, 410, 411, 421
				RETURN 2
			;거실, 툇마루
			CASE 409, 416
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 4
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 41 TO 45
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

