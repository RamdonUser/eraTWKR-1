					RETURN -100
			;가지 않는다
			CASE 423, 424, 429, 430, 432, 437, 439, 440, 442, 443, 445, 448
				RETURN -100
			;그렇게 가지 않는다
			CASE 413, 414, 422, 425, 427, 428
