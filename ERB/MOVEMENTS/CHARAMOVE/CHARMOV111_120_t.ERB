;라바
@CHARAMOVE_DATA_118(ARG, ARG:2)
LOCAL = 118
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;3 요정 가정에 가기 쉽다
			CASE 26 TO 34
				RETURN 5
			;타인의 집이라든지는 가지 않는다
			CASE 408, 410 TO 430, 432, 437, 440, 442, 443, 445, 448, 450 TO 465
				RETURN -100
			;자기 방으로부터의 거리에 의존
			CASE 438, 439
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 3, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 31, 32, 34, 51 TO 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

