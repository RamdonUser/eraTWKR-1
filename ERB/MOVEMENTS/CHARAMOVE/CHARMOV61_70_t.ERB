@CHARAMOVE_DATA_66(ARG, ARG:2)
LOCAL = 66
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		RETURN 0
	;이동 확률
	CASE 2
		;악천후시, 밖이라면 이동하기 쉽고
		SIF OUTROOF(CFLAG:(NO:66):현재위치) && ((TIME:5 & 4) || (TIME:5 & 8))
			RETURN 10
		RETURN 70
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;특히 이루어
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,15, 21,22,23, 61,62,63,67,68,69
