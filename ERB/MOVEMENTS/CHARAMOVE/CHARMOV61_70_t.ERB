	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 9
				RETURN 1
			CASE 2, 7
				SIF DAY:2 == 1 || DAY:2 == 3
					RETURN 1
				RETURN 0
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 23, 71,72,73, 91,92,93,96
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
