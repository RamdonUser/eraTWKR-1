;캐릭터별_각종 데이터
;31~40번까지의 캐릭터
;CALL		USER
;ARG		0:호출 데이터 NO, 1:이동 경향 취득시의 방NO
;RETURN		캐릭터별 호출 데이터치
;COMMENT	
;-----------------------------------------------------------
;사나에
@CHARAMOVE_DATA_31(ARG, ARG:2)
LOCAL = 31
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;모리야신사분사에 자주(잘) 있다
			CASE 21
				RETURN 10
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 71,72,73,76, 81,82,83,84,85
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

