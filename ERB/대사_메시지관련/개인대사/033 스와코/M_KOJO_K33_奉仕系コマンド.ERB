;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K33_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K33_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K33_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		CALL K33_TATARI_COUNTER(3)
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

