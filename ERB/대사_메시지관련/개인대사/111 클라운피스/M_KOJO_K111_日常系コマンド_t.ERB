@M_KOJO_MESSAGE_COM_K111_413_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && DISHASSI == 111
	CALL M_KOJO_COLOR_K111
	;햄버거
	IF DISHNAME == "햄버거"
		IF TCVAR:111:356
			PRINTFORML 「네!햄버거 만들어 주어!」
			IF TALENT:111:연모
				PRINTFORML 「에헤헤, %CALLNAME:MASTER%쉽다∼%UNICODE(0x2764)%」
				PRINTFORML 「함께 작 ♪」
			ELSEIF TALENT:111:사모
				PRINTFORML 「%마스터는% 정말, 쉽구나∼！」
				PRINTFORML 「값에도 도움 시켜!」
			ELSE
				PRINTFORML 「%마스터는% 정말 알고 있는야!」
				PRINTFORML 「값도 도와줄게요!」
			ENDIF
		ELSE
			PRINTFORML 「뭐야 뭐야?  햄버거 만드는 거야?」
			PRINTFORML 「좋잖아~!값도 도와줄게요!」
		ENDIF
		IF ABL:111:요리기능 >= 3
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「그렇다!치즈 버거로 하자!」
				DISHTASTE += "치즈/"
				TCVAR:111:357 = 1
			CASE 2
				PRINTFORML 「그렇다!스파이시 버거로 하자!」
				DISHTASTE += "스파이스/"
				TCVAR:111:357 = 2
			CASE 3
				PRINTFORML 「그렇다!주인님을 코끼리아비스바가를 만들어 보자!」
				DISHTASTE += "스파이스/그림같은/" * 3
				TCVAR:111:357 = 3
			CASEELSE
				PRINTFORML 「그렇다!요정을 이미지 한 페어리 버거로 해 보자!」
				DISHTASTE += "귀엽다/"
				TCVAR:111:357 = 4
			ENDSELECT
		ELSEIF ABL:111:요리기능 >= 1
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「그렇다!치즈 버거로 하자!」
				DISHTASTE += "치즈/"
				TCVAR:111:357 = 1
			CASE 2
				PRINTFORML 「그렇다!스파이시 버거로 하자!」
				DISHTASTE += "스파이스/"
				TCVAR:111:357 = 2
			CASEELSE
				;탈락
			ENDSELECT
		ENDIF
	;과자
	ELSEIF DISHTYPE == 3
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「과자 만들기?」
		CASEELSE
			PRINTFORML 「과자 만들어!」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「값도 주고 싶다!」
		CASE 2
			IF TALENT:111:사모 || TALENT:111:연모
				PRINTFORML 「도움 해 주어!」
			ELSE
				PRINTFORML 「돕기 때문에 값에도 줘!」
			ENDIF
		CASEELSE
			IF TALENT:111:사모 || TALENT:111:연모
				PRINTFORML 「함께 작 ♪」
			ELSE
				PRINTFORML 「값에도 시켜!」
			ENDIF
		ENDSELECT
		;쿠키
		IF DISHNAME == "쿠키"
			PRINTW 
			PRINTFORML 「이것은 주인님으로―, 여기는 친구 같아!」
			TCVAR:111:357 = 1
			IF TALENT:111:연모 && !RAND:8
				PRINTFORML 「아!그러니까, 그것은…%CALLNAME:MASTER%야 만……」
				TCVAR:111:357 = 2
			ENDIF
			PRINTFORML 「어떻게?  닮아 있어?」
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「밥 만드는 거야?」
		CASEELSE
			PRINTFORML 「요리하는 거야?」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「값도 준다―！」
		CASE 2
			IF TALENT:111:사모 || TALENT:111:연모
				PRINTFORML 「도움 해 주어!」
			ELSE
				PRINTFORML 「수전야!」
			ENDIF
		CASEELSE
			IF TALENT:111:사모 || TALENT:111:연모
				PRINTFORML 「함께 작 ♪」
			ELSE
				PRINTFORML 「값에도 시켜!」
			ENDIF
		ENDSELECT
	ENDIF
	PRINTW 
	;야채를 뽑는다
	;야채 빼기 처리는 본체 측에 이동했습니다
	;주인이 뽑고 있든 표시되는 것은 사양이라는 것으로
	IF STRFIND(DISHTASTE, "야채 빼기/") >= 0
		PRINTFORML 「뭇! 이것, 야채 들어가 있다……」
		PRINTFORML 「몰래 뽑아 버려야지!」
		PRINTW 
	ENDIF
	;야채 넣은 야채 빼기(? )(이)라면 안 되기 때문에
	IF STRFIND(DISHTASTE, "햄버거") >= 0 && TCVAR:111:356
		PRINTFORML 「원!　맛있어 보인다∼！」
		PRINTFORML 「당장 먹어요♪ 먹지요?  먹여 줄래?」
	;금단의 요리
	ELSEIF DISHTYPE == 4
		PRINTFORML 「할 수 있었지만……, 뭐야, 이것?」
		PRINTFORML 「저, 정말로 먹는 거야……?」
	ELSE
		IF ABL:111:요리기능 >= 3
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「으로~온 ♪」
				PRINTFORML 「원!맛있어 보인다∼！」
			CASE 2
				PRINTFORML 「값에 걸리면 이런 것이야∼」
				PRINTFORML 「어때?  잘 할 수 있었겠죠?」
			CASE 3
				PRINTFORMW 「할 수 있었다!칭찬하고 칭찬해!」
				PRINTW 　　스윽스윽
				PRINTFORML 「에헤♪」
			CASEELSE
				PRINTFORML 「완성~!의외로 간단했지」
				PRINTFORML 「값요리 잘 되었는지?」
			ENDSELECT
		ELSE
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「―, 지쳤다……」
				PRINTFORML 「요리는 큰 일이구나……」
			CASE 2
				PRINTFORML 「이런 느낌?」
				PRINTFORML 「좀 더 조미료라든지 넣지 않아?」
			CASE 3
				PRINTFORML 「할 수 있었다∼！ 이제(벌써) 이것으로 좋지요!」
				PRINTFORML 「먹으면 변함없다고」
			CASEELSE
				PRINTFORML 「어?  끝?」
				PRINTFORML 「좀 더 조미료라든지 넣지 않아?」
			ENDSELECT
		ENDIF
	ENDIF
	WAIT
ENDIF
RETURN 1
