	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;통상 귀가
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTL
		IF TALENT:연인
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「오늘도  즐거웠어요. 고마워요, 나의 멋진 주인(서방)님」
				PRINTFORMW 농담인 척 하며 말한 란의, 은밀한 빨간 얼굴상이 미소꾸짖었다
			CASE 1
				PRINTFORML 「아무래도, 우리들을 벌써 부부라고 생각하고 있는 사람이 몇 사람이나 있어, 그」
				PRINTFORML 물론, 앞으로도 데이트를 할 때에 점점 증가해 갈 것이다
				PRINTFORMW 언젠가 란이 정색할 때까지는, 이 사랑스러운 수줍고 얼굴을 빌 수 있을 것 같다
			CASE 2
				PRINTFORML 즐거웠던 데이트의 뒤, 귀로의 비탈길에서 두 명은 환상향의 원대한 전망을 만났다
				PRINTFORML 「……그런가, 유카리 님은……」
				PRINTFORMW 군소리를 알아 듣지 못하고, 고개를 갸웃한 %CALLNAME:MASTER%에, 란은 아무것도 아니다고 웃어 돌려주었다
			ENDSELECT
		ELSEIF TALENT:연모
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「상당히 걸었군. 피곤해 있지 않은가, %CALLNAME:MASTER%」
				PRINTFORMW %마스터는% 함 나무를 돌려주어, 그리고 부부 같아교환이라면 아이에게 웃어 걸쳤다
			CASE 1
				PRINTFORML 「이번에는 어디에 가자?  환상향은 넓기 때문에……」
				PRINTFORMW 다가붙어 걸어, 거리낌 없는 회화를 주고 받는 시간이 두 명의 당연하게 되고 있었다
			CASE 2
				PRINTFORML 「이번에는 마요히가로 하루 보낸다는 것은 어때. 늦어지면(자) 자고 가도 좋아」
				PRINTFORMW 즐거운 듯 하는 제안이지만, 서서히 바깥 해자를 매장되어 있을 생각도 드는 %CALLNAME:MASTER%에서 만났다
			ENDSELECT
		ELSEIF TALENT:사모
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……온화한 날이다. 그렇게는 생각하지 않아, %CALLNAME:MASTER%」
				PRINTFORMW 아무래도, 란은 이번 데이트에 만족해 주는 것 같았다
			CASE 1
				PRINTFORML 「내 입장은 알고 있을 것이다. 숨돌리기만 하고 있는 별로도 가지 않야」
				PRINTFORMW 란의 말씨 세치로, %마스터는% 그 안일을돕는다고 할 약속을 장착되어 버렸다
			CASE 2
				PRINTFORML 「아아, 즐거웠다.사실이야」
				PRINTFORMW 문자 그대로의 암컷 여우 그렇다고 해서, %마스터는% 란의 자연스러운 웃는 얼굴을 의심하려고는 생각하지 않았다
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「나쁘지는 않았다. 또 권해 주고」
				PRINTFORMW 과연 그것이 본심인가, %CALLNAME:MASTER%에는 알지 않는다
			CASE 1
				PRINTFORML 「다음의 약속은 할 수 없는데. 나도 여러 가지 일이 있기 때문에」
				PRINTFORMW 아직 아이와의 거리감은 먼 것 같다
			CASE 2
				PRINTFORML 「네가 즐길 수 있던 것이라면 다행히다」
				PRINTFORMW 쪽자신은 어땠어일까, 라고 %마스터는% 생각했다
			ENDSELECT
		ENDIF
		PRINTL 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인 소거 이벤트
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인 2>1 레벨 다운 이벤트
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인 3>2 레벨 다운 이벤트
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 
		RETURN 1
	ENDIF
	PRINTFORML 
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"계 이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;방에서 만남
;==================================================
@M_KOJO_EVENT_K25_1(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	PRINTL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;MASTER가 들어 왔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	;사계 마다 아침 점심 밤
	IF LOCAL:1 && ARG == 1
		;일일일회
		SIF TCVAR:25:350 == 1
			RETURN 1
		IF TALENT:연모
			;DAY:2로 계절을 선택
			SELECTCASE DAY:2
			;춘
			CASE 1
				;TIME:2로 아침 점심 밤을 선택
				SELECTCASE TIME:2
				;조
				CASE 1,2
					PRINTFORML 「%CALLNAME:MASTER%모양. 오늘도 좋은 아침입니다」
					PRINTFORML 「딱딱한 인사는 싫어 하는 거야?」
					PRINTFORMW 근지러운 듯이 하는 %마스터를% 란이 웃고 있는
				;주
				CASE 3,4
					PRINTFORML 「야, %CALLNAME:MASTER%」
					PRINTFORMW 「보내기 쉬운 계절이다. 나가려면 꼭 좋다」
				;야
				CASE 5,6,7
					PRINTFORML 「안녕하세요, %CALLNAME:MASTER%」
					PRINTFORMW 「춘분을 지났다고는 해도, 아직 밤에는 차가워진다. 컨디션에 배려해」
				ENDSELECT
			;하
			CASE 2
				SELECTCASE TIME:2
				;조
				CASE 1,2
					PRINTFORML 「안녕, %CALLNAME:MASTER%」
					PRINTFORML 「아침저녁도 더워지는 계절이다. 어제밤은 잘 수 없지 않았는지?」
					PRINTFORMW 란은 언제라도 시원스러운 생김새다
				;주
				CASE 3,4
					PRINTFORML 「안녕하세요. 오늘도  덥다, %CALLNAME:MASTER%」
					PRINTFORML 「이런, 굉장한 땀. 닦아 하자」
					PRINTFORMW 이마에 뜬 땀을 란이 닦으며 주었다
				;야
				CASE 5,6,7
					PRINTFORML 「%CALLNAME:MASTER%인가. 마르고 건라고 없어 무엇보다」
					PRINTFORML 「날도 저물어 시원해졌군. 있으면 한다면 지금이다」
					PRINTFORMW 여름 철의 얼마 안되는 호기와(뿐)만에 란이 짓이겨 모여 왔다
				ENDSELECT
			;추
			CASE 3
				SELECTCASE TIME:2
				;조
				CASE 1,2
					PRINTFORML 「평안하세요, %CALLNAME:MASTER%」
					PRINTFORMW 「컨디션등 무너뜨리지는 않을 것이다. 이따금은 죽림가 의사에게 진찰받으면 좋다」
				;주
				CASE 3,4
					PRINTFORML 「으응~, 바람을 타고 은행의 향기가」
					PRINTFORML 「분명히 지긋지긋 해 왔다. 도와 주고」
					PRINTFORMW 란이 목덜미에 얼굴을 대어 와, 킁킁하며 코를 울렸다
				;야
				CASE 5,6,7
					PRINTFORML 「야, %CALLNAME:MASTER%」
					PRINTFORML 「이봐, 거기에서 방울 벌레가 울고 있다」
					PRINTFORMW 둘이서 풍류인 음색에 귀를 가라앉혔다
				ENDSELECT
			;동
			CASE 4
				SELECTCASE TIME:2
				;조
				CASE 1,2
					PRINTFORML 「안녕하세요, %CALLNAME:MASTER%……와?」
					PRINTFORML 「뜬금없구나. 별로 상관없겠지만」
					PRINTFORMW 차가워지고 자른 손을 란의 폭신폭신 꼬리에 묻게 해 받았다
				;주
				CASE 3,4
					PRINTFORML 「춥지 않은지, %CALLNAME:MASTER%?」
					PRINTFORML 「그러면, 좀 더 나의 근처에 와라. 심지까지 따뜻하게 해 준다」
					PRINTFORMW 가볍게 란의 꼬리가 목덜미에 감겼다
				;야
				CASE 5,6,7
					PRINTFORML 「―, 차가워져 오고 싶은」
					PRINTFORML 「모피가 있어도 추운 것은 춥다」
					PRINTFORMW 「이런 때는 피부가 제일이야. 그렇게 생각하겠지?」
				ENDSELECT
			ENDSELECT
		ELSEIF TALENT:사모
			PRINTFORML 「%CALLNAME:MASTER%인가. 만날 수 있어 기뻐」
			PRINTFORMW 웃는 얼굴의 란이 친한 척하며 인사를 해 주었다
		ELSE
			PRINTFORML 「야」
			PRINTFORMW 란이 이쪽을 알아차려 손을 들어 왔다
		ENDIF
		TCVAR:25:350 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 들어 왔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:연모
			PRINTFORML 「여기에 있었는지, %CALLNAME:MASTER%」
			PRINTFORML 「으응. 너를 만나러 왔다」
			PRINTFORMW 밝은 상태로 란이 옆에 가까워져 왔다
		ELSEIF TALENT:사모
			PRINTFORML 「오오, %CALLNAME:MASTER%」
			PRINTFORMW 이쪽을 찾아낸 순간, 란의 표정이 확하고 화려해졌다
		ELSE
			PRINTFORML 「이런, 언젠가의」
			PRINTFORMW 란이 아무렇지도 않게 이분에게 인사를 했다
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 함께 들어간다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:연모
			PRINTFORML 「이런, %CALLNAME:MASTER%다」
			PRINTFORML 활짝과 문이 열려, 수건 한 장손에 가진 란이 들어 왔다
			PRINTFORML 얼굴에는 즐거운 듯 하는 미소. 한 걸음 마다 말랑말랑 흔들리는 유방을 숨기려고 하는 헛 스윙은 전혀 없다
			PRINTFORML 「모처럼이고 함께 시켜 받자. 싫다고는 말하지 않든지?」
			PRINTFORMW 두말 할 것 없이 %마스터는% 수긍했다
		ELSE
			PRINTFORML 「이런, 먼저 온 손님이 있었다」
			PRINTFORML 문을 열어 목욕탕에 들어 온 것은 쪽이었다
			PRINTFORML 풍만한 나체에게 타올을 감는 것도 아니고, 흔들리듯 여문 유방을 흔들면서 이분에게 온다
			PRINTFORML 「스페이스는 열려 있고, 옆에 실례해도 상관없어요?」
			PRINTFORMW 물론이다, 라고 %마스터는% 란을 환영했다
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 나와 갔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
			PRINTFORML 문을 열어, 누군가가 목욕탕으로 들어 온다
			PRINTFORML 그것은 작은 수건을 가슴팍에 댄 쪽이었다
			PRINTFORML 기복의 큰 몸을 숨기려면  완전히 가지고 면적이 미만, 유방의 끝도 음부도 노출이다……
			PRINTFORML 「입욕중이었구나. 실례했다」
			PRINTFORMW 조각의 동요도 보이지 않고, 란은 뒤꿈치를 돌려주어 문을 닫았다
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 내쫓아졌다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출처에서 조우했다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;타캐릭터와 데이트중에 조우했다
	;ARG:1에 데이트상대
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 
		RETURN 1
	ENDIF
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;아침에 일어 났을 때(안녕 말)
;==================================================
@M_KOJO_EVENT_K25_2(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;비동거(同棲) 한편 상대가 먼저 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET 방에 MASTER가 들어가 있다(연모시 동침중취침시만?
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER 방에 TARGET가 들어 오고 있다(연모 없음에서도 들어 올 가능성 있어
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;공통
		IF TALENT:연모 
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「, 일어났다」
				PRINTFORML 눈시울을 열면(자), 히죽히죽 웃는 얼굴의 란이 가까운 거리로 들여다 봐 붐비고 있었다
				PRINTFORML 「후후후, 조금 못된 장난해 지나쳤는지?」
				PRINTFORMW 자신이 자고 있는 동안에 도대체 무엇을 했었던 것인가
			;아침 펠라를 해 보고 싶었다
			CASE 1
				SIF TALENT:MASTER:성별 != 2
					RETURN 1
				PRINTFORML 무엇인가, 이상한 감각이 상체를 달려 깨어난다
				PRINTFORML 부짝부짝이라고 하는 물소리. 누군가가 %CALLNAME:MASTER%의 고간에 얼굴을 파묻고,  열심히 귀두를 빨고 있는 것 같다
				PRINTFORML 「안녕, %CALLNAME:MASTER%. 좋은 꿈은 볼 수 있었는지?」
				PRINTFORML 양물에 뺨에 뺨을 비비는을 하는 란이 그렇게 말해, 눈을 치켜 뜨고 봄의 웃는 얼굴로 고기 줄기에 혀를 기게 한다
				PRINTFORMW 자고 있는 동안 얼마나 빨아지고 있던 것이든지. 곧%마스터는% 잠 깨고의 일발을 란의 입안에 토해냈다
				BASE:MASTER:정력 -= 100
			;구슬 빨고도 있겠어
			CASE 2
				SIF TALENT:MASTER:성별 != 2
					RETURN 1
				PRINTFORML 일어난 순간 %마스터는% 간지러움에 몸을 비비 꼰다
				PRINTFORML 급격하게 의식이 깨어, 누군가가 자신의 고간에 못된 장난을 하고 있는 것에 깨달았다
				PRINTFORML 「응, 아야」
				PRINTFORML 혀미만에 란이 말하면(자), 입속에 뺨 펴진 오른쪽의 불알에 진동이 영향을 주었다
				PRINTFORML 흘립 한 페니스의 끝에서 쿠퍼가 늘어져 실을 당기면서 란의 콧날에 착지했다
				PRINTFORML 아무래도 란은 자명종 대신에 단지 %CALLNAME:MASTER%의 불알을 빨고 있던 것 같다……
				PRINTFORMW 마지막 최후, 란은 다 모여 뿜어 나온 앙상을 목구멍으로 받아 들였다
				BASE:MASTER:정력 -= 100
			ENDSELECT
		ELSE
			PRINTFORML 「안녕하세요. 잘 자고 있었던, %CALLNAME:MASTER%」
			PRINTFORMW 깨어나면(자), 옆에서 란이 정좌하고 있었다
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;동거(同棲) 한편 상대가 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:연모 
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「, 일어났다」
				PRINTFORML 눈시울을 열면(자), 히죽히죽 웃는 얼굴의 란이 가까운 거리로 들여다 봐 붐비고 있었다
				PRINTFORML 「후후후, 조금 못된 장난해 지나쳤는지?」
				PRINTFORMW 자신이 자고 있는 동안에 도대체 무엇을 했었던 것인가
			;아침 펠라를 해 보고 싶었다
			CASE 1
				SIF TALENT:MASTER:성별 != 2
					RETURN 1
				PRINTFORML 무엇인가, 이상한 감각이 상체를 달려 깨어난다
				PRINTFORML 부짝부짝이라고 하는 물소리. 누군가가 %CALLNAME:MASTER%의 고간에 얼굴을 파묻고,  열심히 귀두를 빨고 있는 것 같다
				PRINTFORML 「안녕, %CALLNAME:MASTER%. 좋은 꿈은 볼 수 있었는지?」
				PRINTFORML 양물에 뺨에 뺨을 비비는을 하는 란이 그렇게 말해, 눈을 치켜 뜨고 봄의 웃는 얼굴로 고기 줄기에 혀를 기게 한다
				PRINTFORMW 자고 있는 동안 얼마나 빨아지고 있던 것이든지. 곧%마스터는% 잠 깨고의 일발을 란의 입안에 토해냈다
				BASE:MASTER:정력 -= 100
			;구슬 빨고도 있겠어
			CASE 2
				SIF TALENT:MASTER:성별 != 2
					RETURN 1
				PRINTFORML 일어난 순간 %마스터는% 간지러움에 몸을 비비 꼰다
				PRINTFORML 급격하게 의식이 깨어, 누군가가 자신의 고간에 못된 장난을 하고 있는 것에 깨달았다
				PRINTFORML 「응, 아야」
				PRINTFORML 혀미만에 란이 말하면(자), 입속에 뺨 펴진 오른쪽의 불알에 진동이 영향을 주었다
				PRINTFORML 흘립 한 페니스의 끝에서 쿠퍼가 늘어져 실을 당기면서 란의 콧날에 착지했다
				PRINTFORML 아무래도 란은 자명종 대신에 단지%CALLNAME:MASTER%의 불알을 빨고 있던 것 같다……
				PRINTFORMW 마지막 최후, 란은 다 모여 뿜어 나온 앙상을 목구멍으로 받아 들였다
				BASE:MASTER:정력 -= 100
			ENDSELECT
		ELSE
			PRINTFORML 「안녕하세요. 잘 자고 있었던, %CALLNAME:MASTER%」
			PRINTFORMW 깨어나면(자), 옆에서 란이 정좌하고 있었다
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;동거(同棲) 한편 상대가 자고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTER가 일어나고 있어, 상대가 일어났다
	;동침 때 TARGET 기상은 이쪽(TARGET 쇠약 or 동침중TARGET 피곤함 패배 후 그대로 MASTER가 계속 일어난 다음 상대가 기상했을 경우 한정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;비동거(同棲)
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;동거(同棲) 안
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;공통
		IF TALENT:연모
			PRINTFORML 「안녕―」
			PRINTFORML 자고 일어난 좋은 아이도, 깨어난 직후는 이와 같이 처리가 무디어지고 있다
			PRINTFORMW 흔들흔들(살랑살랑) 흔들리는 여우귀 달린 머리를 쓰다듬고 있고 했다
		ELSE
			PRINTFORML 「……안녕. 빠른데」
			PRINTFORML 「어쩌면 나의 잠자는 얼굴을……이나, 아무것도 아니다」
			PRINTFORMW 란은 눈을 벗어나, 허겁지겁 아침의 몸치장을 시작했다
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;그 외
	PRINTFORML  
ENDIF
RETURN 1


;==================================================
;밤에 잘 때(휴업(휴가) 말)
;==================================================
@M_KOJO_EVENT_K25_3(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------

IF LOCAL
	PRINTL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;자기 방에 돌아간다(정시가 되어 잔다)
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;신사 더부살이시 전용과 상정해 기술
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「나는 이제 쉬게 해 받아. %CALLNAME:MASTER%도 그다지 밤샘을 하지 않게」
			PRINTFORMW 방으로 돌아가는 란을 전송했다
		CASE 1
			PRINTFORML 「그럼,  주인(서방)님. 먼저 실례 하겠습니다」
			PRINTFORMW 일부러 송구해한 인사를 해 란은 방에 돌아와 갔다
		CASE 2
			PRINTFORML 「그럼. 또 내일」
			PRINTFORMW 깨끗이 휴가의 키스를 하며,  란은 자기 방에 돌아와 갔다
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;졸린 듯이 하고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:연모 || TALENT:사모
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……………」
				PRINTFORMW 「미안. 의식이 날았다」
			CASE 1
				PRINTFORML 일견 평상시 그대로의 모습이지만, 무척 동작이 적다
				PRINTFORMW 아이도 졸음에는 이길 수 없는 것 같다
			CASE 2
				PRINTFORMW 란의 꼬리가 천천히 아래에 내려 간다. 상당히 졸린 싸인이다
			ENDSELECT
		ELSE
			PRINTFORML 「우웅, 으응」
			PRINTFORMW 맞장구가 적당이다. 이제 졸려지는 시간인것 같다
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;수면중(일어나지 않기 때문에 회화 라기 보다는 잠꼬대? )
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:연모 || TALENT:사모
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;장난중
	;기입 체크(=0, 비표시, 1, 표시)
	;LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG == 4
		;PRINTFORMW 
		;RETURN 1
	;ENDIF
	;-------------------------------------------------
	;쇠약중
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF TALENT:연모 || TALENT:사모
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 란이 개인전과%CALLNAME:MASTER%의 어깨로 하고 눈사태 걸린다
				PRINTFORMW 그렇게도의 그녀도 체력의 한계인것 같다
			CASE 1
				PRINTFORML 탐스러운 털이 얼마인가 위 보고, 꼬리의 각도가 서투르다는 것은 왔다
				PRINTFORMW 지금은 이제(벌써) 란의 상태를 한눈에 안다. 피로한 그녀의 손을 잡아 당겨 주었다
			CASE 2
				PRINTFORML 「응, 지쳤다. 조금 쉬게 해 주고」
				PRINTFORMW 약점을 보였지만들 없는 쪽이지만, %마스터는% 이미 예외다
			ENDSELECT
		ELSE
			PRINTFORMW 「……후우. 이 근처에서 끝맺을까」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTER가 다운(방까지 옮겨진다)
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF TALENT:연인
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「잘자, %CALLNAME:MASTER%」
				PRINTFORMW 피곤함에 항원않다, 란의 팔에 몸을 맡겼다
			CASE 1
				PRINTFORML 「괜찮아. 후의 처리는 나에게 맡겨라」
				PRINTFORMW 그렇다면 안심이라면%마스터는% 눈시울을 닫았다
			CASE 2
				PRINTFORML 「수고 하셨습니다」
				PRINTFORMW 등에 안아 멈추었다 팔을, 이마에 입술의 감촉을 느꼈다
			ENDSELECT
		ELSEIF TALENT:연모
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「그래그래. 옮겨 주기 때문에 나에게 잡아지고」
				PRINTFORMW 란은 쉽게와%마스터를% 키에 업었다
			CASE 1
				PRINTFORML 「조금 무리를 시켰는지. 미안하다」
				PRINTFORMW 당장이라도 의식을 잃을 것 같은%마스터를%, 많은 꼬리 이 살짝 지지했다
			CASE 2
				PRINTFORML 「자주(잘) 노력했다」
				PRINTFORMW 정신을 잃기 전, 란의 상냥한 미소가 눈에 비쳤다
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

