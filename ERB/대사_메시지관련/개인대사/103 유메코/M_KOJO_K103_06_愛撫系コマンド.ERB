RETURN RESULT

@M_KOJO_MESSAGE_COM_K103_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;1行入れたい
	PRINTFORML 
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「아, 조금, 이나 , 낯간지럽다……응…」
		PRINTFORML 
		PRINTFORMDW %CALLNAME:MASTER%의 애무가 간지러운 것인지, 몽자는 몸을 비틀고 있다
		PRINTFORMDW 그러나 때가 지나는 것에 따라, 점차 몽자의 숨이 난폭하게 되어 간다…
		PRINTFORMDW 토지 분할과 습기를 띤 음부를%M_KOJO_K103_ZURASHI()%손가락으로 살그머니 위를 덧쓰면(자), 작은 허덕여 소리를 흘렸다…
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML
			CALL M_KOJO_K103_CALL_ZAMEN()
			RETURN 1
		ELSE
			PRINTFORMW 「 나의%M_KOJO_K103_GET("ヴァギナ")%, 말하고 있는……」
			;RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML
			CALL M_KOJO_K103_CALL_ZAMEN()
			RETURN 1
		ELSE
			PRINTFORMW 「%M_KOJO_K103_GET("アヌス")%…, 아″……」
			;RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		SELECTCASE M_KOJO_K103_RANDNR(8, 911)
		CASE 1
			PRINTFORMW 「아,……응…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「 좀 더……좀 더 손대어……%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「후~……응…귀 약한거야…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「%M_KOJO_K103_AEGI()%……%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「아, 거기 기분이 좋다…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「 좀 더……좀 더……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「아 응……손대는 방법 외야인것 같다…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「아…그렇지만………좋은 것, 기분 좋아서…아…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「후~………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「여기, 여기, 좀 더 만지작거려……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「팥고물,………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「아, 아, 안돼, 구리를 그렇게……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「응응응…가슴, 가슴 좀 더 손대어…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「아…기분 있고……%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「 좀 더 여러 가지 곳 만지작거리기를 원하는거야…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「아…그래……아 응……%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	;屈服3
	;ELSEIF MARK:괘씸각인 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		SELECTCASE M_KOJO_K103_RANDNR(8, 911)
		CASE 1
			PRINTFORMW 「아,……응…」
			PRINTFORMW 「아, 거기 기분이 좋다……%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「후~……응…이…」
			PRINTFORMW 「%M_KOJO_K103_AEGI()%……%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「아, 거기, 거기…」
			PRINTFORMW 「아…아……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「아 응……손대는 방법 외야인것 같다…」
			PRINTFORMW 「아…그렇지만………기분 좋아서…아…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「후~………」
			PRINTFORMW 「여기, 여기, 여기가 기분이 좋은거야……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「팥고물,………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「아, 안돼, 구리를 그렇게……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「응응…가슴 느껴버린다…」
			PRINTFORMW 「아…기분 있고……%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「체내 만지작거려져…응응」
			PRINTFORMW 「아…안돼……아 응……%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
