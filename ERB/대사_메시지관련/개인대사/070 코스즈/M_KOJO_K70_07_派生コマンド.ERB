;9,ダブルフェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K70_9
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K70_9_1
;戻り値とTARGETを保存、TARGETを二人目に一時的に交代して二人目口上を呼び出した後、TARGETをもどして保存した戻り値でRETURNする
;二人目はいらない場合はここで処理
;二人目
LOCAL = MASTER_POSE(4,1,1)
LOCAL:1 = RESULT
LOCAL:2 = TARGET
TARGET = LOCAL
TRYCALLFORM M_KOJO_MESSAGE_SCOM_K{TARGET}_9_2
TARGET = LOCAL:2
RETURN LOCAL:1

;一人目口上
@M_KOJO_MESSAGE_SCOM_K70_9_1
#DIM CHARA
CHARA = MASTER_POSE(4,1,1)
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = CFLAG:70:1006
;-------------------------------------------------
IF LOCAL
	IF CHARA == 80
		SELECTCASE M_KOJO_K70_BALANCE(70, 80)
		CASE 8, 5
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「아구, 함께%CALLNAME:MASTER%씨에게 봉사합시다%UNICODE(0x2764)%」
			CASEELSE
				PRINTFORMW 「후후%UNICODE(0x2764)% 아구와는 만큼 반죽하고%UNICODE(0x2764)%」
			ENDSELECT
		CASE 7, 6
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「실마리, 상무야 아구！ 어느 쪽인가%CALLNAME:MASTER%씨를 기분 좋게하게 할까…！」
			CASEELSE
				PRINTFORMW 「응응……아구뿐 간사해요……！ 나래……！」
			ENDSELECT
		CASE 4, 3
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「아구보다 기분 좋게 해 주어요！」
			CASEELSE
				PRINTFORMW 「아구에는 주지 않기 때문에…！ 짠다%UNICODE(0x2764)%」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「응응……%CALLNAME:CHARA%산파빚 간사합니다……！ 나래……！」
		CASEELSE
			PRINTFORMW 「응-！ %CALLNAME:CHARA%씨, 나에게도 주세요……！」
		ENDSELECT
	ENDIF
	;サディスティックになる
	IF NOWEX:MASTER:사정
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「…………그리고 인가？ 말할 수 야, 가득 아히라고 구아는 있고%UNICODE(0x2764)%」
			PRINTFORMW 「응%UNICODE(0x2764)% %UNICODE(0x2764)% 응%UNICODE(0x2764)% 응%UNICODE(0x2764)% 응%UNICODE(0x2764)% 응%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「%CALLNAME:MASTER%씨, 이렇게 닦는들 하물며……%UNICODE(0x2764)% 내가 흡수해 주어요%UNICODE(0x2764)%」
			PRINTFORMW 「%UNICODE(0x2764)% %UNICODE(0x2764)% %UNICODE(0x2764)% %UNICODE(0x2764)% …%UNICODE(0x2764)% %UNICODE(0x2764)% %UNICODE(0x2764)%」
		CASEELSE
			PRINTFORMW 「%UNICODE(0x2764)% %UNICODE(0x2764)% 응%UNICODE(0x2764)% 있고 인가？ …%UNICODE(0x2764)% 알았습니다…%UNICODE(0x2764)%」
			PRINTFORMW 「%ONEDARI_MASTER_TINKO()%보는 구 가득 주세요%UNICODE(0x2764)% %UNICODE(0x2764)% %UNICODE(0x2764)% %UNICODE(0x2764)% 」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「%UNICODE(0x2764)% …%UNICODE(0x2764)% 짠다%UNICODE(0x2764)% %UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「…%UNICODE(0x2764)% %UNICODE(0x2764)% %UNICODE(0x2764)% 후~%UNICODE(0x2764)% %UNICODE(0x2764)%」
		CASEELSE
			PRINTFORMW 「인가～？ 끈인가～？ %UNICODE(0x2764)% %UNICODE(0x2764)% %UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
ENDIF
