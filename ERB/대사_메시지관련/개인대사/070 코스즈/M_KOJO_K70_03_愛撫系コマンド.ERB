		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「%CALLNAME:MASTER%씨……, 빨아 주세요……\/\/\/」
		CASE 2
			PRINTFORMW 「%CALLNAME:MASTER%씨……나의 불쾌한 곳……빨아 주세요……\/\/\/」
		CASEELSE
			PRINTFORMW 「%CALLNAME:MASTER%씨……, 빨아……%UNICODE(0x2764)%」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「응%UNICODE(0x2764)% 아%UNICODE(0x2764)% 아%UNICODE(0x2764)%」
			PRINTFORMW 「좋습니다%UNICODE(0x2764)% 좀 더%UNICODE(0x2764)% 아%UNICODE(0x2764)%」
