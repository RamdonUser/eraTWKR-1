	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		PRINTFORMW 
		RETURN 1
	ENDIF
	IF FLAG:시간정지
	;継続クンニ
	ELSEIF PREVCOM == SELECTCOM
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「은 원%UNICODE(0x2764)% 아%UNICODE(0x2764)% 아%UNICODE(0x2764)%」
