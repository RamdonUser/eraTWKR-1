;==================================================
;0,愛撫
;==================================================
@M_KOJO_MESSAGE_COM_K122_0
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K122_0_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K122_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:광기
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「그렇게, 그런 느낌으로……후후,%CALLNAME:MASTER%도 능숙하다―%UNICODE(0x2764)%」
			PRINTFORMW 무용은%CALLNAME:MASTER%의 애무에 황홀로 한 미소를 띄우고 있다.
		CASE 1
			PRINTFORML 「네가 마음대로 해도 좋아？이봐요%UNICODE(0x2764)%」
			PRINTFORMW 무용은 조금 몸을 움직여,%CALLNAME:MASTER%하지만 손대기 쉽게 해 주었다.
		ENDSELECT
	;恋慕
	ELSEIF TALENT:연모
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「%CALLNAME:MASTER%에 손대어지면(자), 몹시 두근두근 한다……%UNICODE(0x2764)%」
			PRINTFORMW 무용은 숨을 황면서,%CALLNAME:MASTER%(으)로부터 주어지는 쾌락을 향수하고 있다.
		CASE 1
			PRINTFORML 「, 응응%UNICODE(0x2764)% %CALLNAME:MASTER%, 기분이 좋다…%UNICODE(0x2764)%」
			PRINTFORMW 무용은%CALLNAME:MASTER%의 애무에 황홀로 한 미소를 띄우고 있다.
		ENDSELECT
	RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「응……후후, 나쁘지는 않을까…%UNICODE(0x2764)%」
			PRINTFORMW 무용은%CALLNAME:MASTER%의 애무에 만족기분인 미소를 띄웠다.
		CASE 1
			PRINTFORML 「 좀 더, 해도 좋아？%UNICODE(0x2764)%」
			PRINTFORMW 무용은%CALLNAME:MASTER%(을)를 권하도록(듯이) 신체를 밀착시켰다.
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
