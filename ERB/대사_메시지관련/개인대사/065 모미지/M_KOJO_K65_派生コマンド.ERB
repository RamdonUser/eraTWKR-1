;==================================================
;12,パイズリフェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K65_12
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K65_12_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K65_12_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:발정
			PRINTFORML 「…구와 젖가슴으로 함께…？」
			CALL HPH_PRINT,@"「あは、%CALLNAME:MASTER%さんのえっち…HPH」","L"
			PRINTFORML 화는%CALLNAME:MASTER%의 반응을 치뜬 눈보람에 방문하면서, 천천히 페니스를 가슴의 사이에 넣었다.
			PRINTFORMW 매끄러운 피부가 민감한 부분와 관련되어 다해 실로 좋은 경치다.
			CALL HPH_PRINT,@"「……HPH%K65_AEGI("控えめ",4)%」","L"
			PRINTFORMW %CALLNAME:MASTER%의 반응에 기분을 자주(잘) 한 화는 고혹적인 미소를 띄워 귀두에 키스를 반복했다…
		ELSE
			PRINTFORML 「…사이에 둔 채로, 빱니까…？」
			PRINTFORMW 「어라고 알았습니다, 해 보겠습니다…」
			PRINTFORML 화는%CALLNAME:MASTER%의 얼굴을 들여다 봐 붐비어, 천천히 페니스를 가슴의 사이에 넣었다.
			PRINTFORMW 매끄러운 피부가 민감한 부분와 관련되어 다해 실로 좋은 경치다.
			CALL HPH_PRINT,@"「ん…これで、いいみたいですね…%K65_AEGI("控えめ",3)%」","L"
			PRINTFORMW 화는%CALLNAME:MASTER%의 반응을 치뜬 눈보람에 방문하면서, 약간 사양 쉽상으로 귀두에 입맞춤하고하기 시작했다…
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;技量不足
	ELSEIF ABL:TARGET:가슴 < 2
		IF TCVAR:발정
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「응응…, 응？…」
					PRINTFORML 화는%CALLNAME:MASTER%의 귀두에 달라붙으려 하고 있지만,
					PRINTFORMW 가슴의 사이에 뛰어 도는 소레를 꽤 잡히지 않고, 불만 그렇게 입술을 날카롭게 할 수 있고 있다…
				CASE 1
					PRINTFORML 「응실마리, 구,…」
					PRINTFORML 화기울 수 있는에 페니스를 가슴으로 문지르지만, 그녀가 초조해 할 정도로 페니스의 취급이 엉성하게 되어,
					PRINTFORMW 오히려%CALLNAME:MASTER%(은)는 절정으로부터 멀어져 버린다…
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「응실마리,…응, ,……」
					PRINTFORML 화는 가슴을 페니스에 문지르거나 귀두에 키스 해 보거나와 노력하고 있다.
					PRINTFORMW 미안할 정도  노력하고 있는 것은 알지만…
				CASE 1
					PRINTFORMW 「이것, 상당히 어렵네요……」
			ENDSELECT
		ENDIF
	;発情
	ELSEIF TCVAR:발정
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			IF ABL:정액중독 > 3 && RAND:2
				SELECTCASE RAND:3
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「っ…、じゅるるっHPH%K65_AEGI("フェラ音",4)%」","L"
						CALL HPH_PRINT,@"（くるっ…HPH%CALLNAME:MASTER%さんの%TEXTR("とろとろ/えろえろ/")%%TEXTR("せーえき/せーし/")%HPH）","W"
					CASE 2
						CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
						PRINTFORML %CALLNAME:MASTER%의 절정을 예감 해, 화는 가슴에서의 압박을 한층 더 강하게 했다.
						PRINTFORMW 에로 한 유육이 음경에 핏치리와 밀착해, 도망갈 장소가 없는 쾌락이 덮쳐 온다.
						CALL HPH_PRINT,@"「ふっHPHふーっHPH%K65_AEGI("フェラ音",3)%…んんっ…HPH」","L"
						PRINTFORML 화는 혀를 날카로워지게 해 정낭으로부터 치솟아 오는 물건을 파내는것 같이 령구를 빤다.
						PRINTFORMW %CALLNAME:MASTER%(은)는 격렬한 쾌감에 신체를 굳어지게 한이면서, 화의 구강에 충분히열 자백하기 시작했다…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「あっ…HPH%CALLNAME:MASTER%さん、イッちゃいそうなんですか…？」","L"
						CALL HPH_PRINT,@"「…おちんちんにちゅーしててあげますから、このままおっぱいでイッてくださいね…HPH」","W"
				ENDSELECT
			ENDIF
		;勃起しきってない
		ELSEIF BASE:MASTER:발기 < 1000
			;SELECTCASE RAND:2
			;	CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
			;	CASE 1
			;		
			;ENDSELECT
		ELSE
			SELECTCASE RAND:4
				CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_82_1
				CASE 1
					CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
					PRINTFORMW 부드러운 유방에 페니스를 근원으로부터 켜 거론되어 장대 앞을 향해 강렬한 쾌감이 치솟는다.
					CALL HPH_PRINT,@"「ふっHPHんんっ…HPH%K65_AEGI("フェラ音")%」","L"
					PRINTFORMW 오르는 쾌락은, 눈깔사탕을 빨고 녹이는 것 같은 설견 있고로 귀두 전체로 발라 넓힐 수 있어 간다.
					CALL HPH_PRINT,@"「んっHPHあは…おちんちんのニオイ凄くて、ボーッとしてきちゃいます…HPH」","L"
					PRINTFORMW 열을 띤 것 같은 화의 소리에,%CALLNAME:MASTER%의 사고까지도 녹여져 가는 것 같았다…
				CASE 2
					CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%」","L"
					PRINTFORMW 화는 기분 좋은 소리를 내, 가슴의 골짜기로부터 들여다 보는 귀두에 리듬 자주(잘) 입 맞춤을 반복하고 있다…
				CASE 3
					CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
					CALL HPH_PRINT,@"「はあHPHんっ…HPH%K65_AEGI("フェラ音",3)%」","L"
					CALL HPH_PRINT,@"「ん、%CALLNAME:MASTER%さん…%K65_AEGI("控えめ",2)%…もっと、おつゆ、飲ませてください…HPH」","L"
			ENDSELECT
		ENDIF
	;非発情
	ELSE
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			IF ABL:정액중독 > 3 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
						PRINTFORML 착정에 열중하고 있는지, 화는 말을 발표할 것도 없고%CALLNAME:MASTER%의 페니스에 달라붙어서 놓지 않고 있다.
						PRINTFORMW 넘쳐 흐름 떨어진 타액으로 미끄러짐의 좋아진 모치모치의 피부가, 장대의 근원으로부터 첨단와 관련되어 붙어 기분 좋다.
						PRINTFORML 부풀어 오르는 쾌락이 정점으로 향하는 순간, 화는 빵 빵에 부풀어 오른 물건을 따뜻한 구강내의 안쪽까지 조용히 맞아들였다…
						CALL HPH_PRINT,@"「%K65_AEGI("フェラ音",3)%…んっ…んんっ…HPH」","W"
						PRINTFORMW 한계까지 진해진 모취를 입 가득 포함한 화는, 스스로도 달했는지와 같이인 체하는 와 떨렸다…
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						PRINTFORML 「…아, 이있을 것 같네요,%CALLNAME:MASTER%씨…」
						CALL HPH_PRINT,@"「それじゃあ、このままおっぱいで擦りながら、舐めてあげますね…HPH」","L" 
						CALL HPH_PRINT,@"「ん、ちゅ…HPH%K65_AEGI("フェラ音")%」","W"
					CASE 2
						PRINTFORML 「…응,…젖가슴, 기분이 좋습니까…？」
						CALL HPH_PRINT,@"「ふふ、我慢しなくていいですから、いっぱい射精してくださいね…HPH」","L" 
						CALL HPH_PRINT,@"「ちゅむ…%K65_AEGI("フェラ音")%…HPH」","W"
				ENDSELECT
			ENDIF
		;勃起しきってない
		ELSEIF BASE:MASTER:발기 < 1000
			;SELECTCASE RAND:2
			;	CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
			;	CASE 1
			;		
			;ENDSELECT
		ELSE
			SELECTCASE RAND:4
				CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_82_1
				CASE 1
					CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
					PRINTFORMW （%CALLNAME:MASTER%씨, 기분이야 유혹한다…）
					;[献身的]だからね。セフレだろうともっと気持ちよくなってほしいはずだよね。
					SIF RAND:2
						CALL HPH_PRINT,@"（もっと、悦んでもらいたいな…HPH）","W"
				CASE 2
					CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%」","L"
					PRINTFORMW 화는 기분 좋은 소리를 내, 가슴의 골짜기로부터 들여다 보는 귀두에 리듬 자주(잘) 입 맞춤을 반복하고 있다…
				CASE 3
					CALL HPH_PRINT,@"「わ、ぁ…胸の間で、ぴくぴくして……んむっ…%K65_AEGI("控えめ",2)%」","L" 
					PRINTFORML 화는 뛰어 도는 페니스의 첨단을 입술로 잡아 육 간을 열심히 가슴으로 잡아당겼다…
					CALL HPH_PRINT,@"（うう…コスるたびに、%CALLNAME:MASTER%さんのにおいが強くなって…おかしくなっちゃいそう…HPH）","W"
			ENDSELECT
		ENDIF
	RETURN 1
	ENDIF
ENDIF
