						CALL HPH_PRINT,@"「%K65_AEGI()%」", "W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI(,2)%…んっ…」", "L"
						PRINTFORMW 입맞춤을 주고 받으면서 손가락 그대로가 좋은 머리카락을 어루만지면(자), 화는\@ TALENT:연모 && RAND:2 ? トロンとした目で見つめてきた # 嬉しそうに微笑んだ \@.
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1


