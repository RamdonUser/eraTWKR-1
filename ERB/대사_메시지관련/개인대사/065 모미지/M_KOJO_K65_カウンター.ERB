@M_KOJO_MESSAGE_COUNTER_K65_88
CALL EVENT_COUNTER_MESSAGE
CALL M_KOJO_MESSAGE_COUNTER_K65_88_1
RETURN RESULT
@M_KOJO_MESSAGE_COUNTER_K65_88_1
;こなれてない
;そもそもコレ、出ないんじゃ…？
IF ABL:V감각 < 2
	IF TCVAR:발정
		CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
		PRINTFORML 질질허리를 거절하는 화의 얼굴이 때때로 비뚤어진다.
		PRINTFORML 발정하고 있다고는 해도, 역시 아직 익숙해지지 않겠지…
	ELSE
		PRINTFORML 「어떻게…입니까？」
		PRINTFORML 「기분이 좋습니까…？」
		PRINTFORML %CALLNAME:MASTER%(은)는 기특하게 허리를 거절하는 그녀의 머리를 어루만져 주었다.
	ENDIF
ELSE
	IF TCVAR:발정
		SELECTCASE RAND:4
			CASE 0
				CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
			CASE 1
				CALL HPH_PRINT,@"「%K65_AEGI(,4)%…あはHPHおちんちんカチカチぃ…HPH」","L"
				CALL HPH_PRINT,@"「はっ、あぁっHPHおくまで、ほじほじされてっ…きもちいいっ…HPH」","L"
				PRINTFORML 화는 이마에 땀을 배이게 한이면서 열심히 허리를 털고 있다…
			CASE 2
				PRINTFORML 화는 사냥감을 짜 깐 짐승과 같이 난들과 눈을 빛낼 수 있는이면서 쾌락을 탐내고 있다…
				CALL HPH_PRINT,@"「%K65_AEGI(,2)%…きもちいっ…きもちいいよぉっ…HPH」","L"
			CASE 3
				PRINTFORML 주와 같은 땀을 이마에 배이게 한 화가 추송을 반복할 때에, 와 추잡한 소리가 울린다…
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
			CASE 1
				CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
				PRINTFORML 「어떻게…입니까？…기분 좋은, 입니까？」
				PRINTFORML 기분이 좋다고 대답하면(자), 화의 귀가 기쁜듯이 피콘과 뛰었다.
				CALL HPH_PRINT,@"「%K65_AEGI(,2)%…わたしも…%K65_AEGI(,2)%…きもちひっ、です…HPH」","L"
			CASE 2
				PRINTFORML 「그…%CALLNAME:MASTER%씨……」
				PRINTFORML 화가%CALLNAME:MASTER%의 수를 잡아, 손가락의 가랑이로 손가락끝을 들어가게 할 수 있다.
				CALL HPH_PRINT,@"「…ふふ。%CALLNAME:MASTER%の手、温かいです…」","L"
				PRINTFORML 애인 막간에 한 손을 꽉 잡아 닫아 화는 다시 추송을 시작했다…
				CALL HPH_PRINT,@"「%K65_AEGI()%」","L"
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

