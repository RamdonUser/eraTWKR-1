CALL M_KOJO_MESSAGE_COM_K65_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,フェラチオ
;==================================================
@M_KOJO_MESSAGE_COM_K65_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(81)
		IF TCVAR:발정
			PRINTFORML 「납치…？」
			PRINTFORML 「구로 하고 가지고 싶습니까？」
			PRINTFORML 「키득, 알았습니다. 그러면, 자지 내 주세요…」
			PRINTFORML 화는 낄낄 웃으면서,%CALLNAME:MASTER%의 아랫배로 얼굴을 접근했다.
			CALL HPH_PRINT,@"「くんくん…ふふ、凄いにおい…私も興奮してきちゃいます…ちゅっHPH」", "L"
			PRINTFORMW 페니스에 가볍게 키스를 하면(자), 화는 음미에 미소지어 혀를 내밀어, 첨단을 치로 치로와 빨기 시작했다…
		ELSE
			PRINTFORML 「…！？」
			PRINTFORML 「…저, 네와…빨면, 좋습니까…」
			PRINTFORML 입봉사에의 기대에 실룩실룩떨리는 페니스를 응시하면(자), 화는 꿀꺽군침이 돌아 침을 감추어 붐비었다.
			PRINTFORML 분명하게 긴장하고 있는 모습이지만, 거부할 생각은 없는 것 같다.
			PRINTFORML 「저, 실례합니다, 군요…」
			PRINTFORML （아, 굉장한 냄새…이것이,%CALLNAME:MASTER%씨의…）
			PRINTFORMW 화는 흠칫흠칫 혀를 쑥 내밀어, 사양 쉽상으로 페니스의 첨단을 빨기 시작했다…
		ENDIF
		RETURN 1
	ENDIF
	;初めてのびゅう
	IF NOWEX:MASTER:11 && (EXP:정음경험 == 0 || TCVAR:정음경험 == 0)
		PRINTFORML 「응응″…！？…！」
		PRINTFORML 절정의 예감에 벌벌떨리는 음경에 놀랐는지, 화는 빨고 있던 귀두를 입으로부터 떼어 놓아 버렸다.
		PRINTFORMW 그 자극이 마지막 인철이 되어,%CALLNAME:MASTER%의 페니스로부터 힘차게 정액이 분출했다…
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;発情中
	ELSEIF TCVAR:발정
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			;慣れてない
			IF EXP:정음경험 < 20
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「응, ,…내자입니까…？」
						CALL HPH_PRINT,@"「いいですよ、このまま、口に…HPH」", "W"
					CASE 1
						CALL HPH_PRINT,@"「ちゅ、れるれるっ……ちゃんと、口の中にくださいね…HPH」", "L"
						CALL HPH_PRINT,@"「がんばって、飲んでみます、から…HPH」", "W"
				ENDSELECT
			;精液中毒高め
			ELSEIF ABL:정액중독 > 3 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「……！！ちゅぶっ、にゅじゅうううう～～～っHPH」", "L"
						PRINTFORML 사정의 전조를 감지한 화는,%CALLNAME:MASTER%의 허리를 억눌러 후오로 페니스를 념 글자 붐비었다.
						CALL HPH_PRINT,@"（だしてっHPHだしてっHPHせーえき、はやくっ…HPH）", "L"
						PRINTFORML 화는 붉은 눈동자를 기대에 빛낼 수 있는이면서, 눈을 치켜 뜨고 봄에 이쪽을 올려봐 사정을 조른다.
						PRINTFORMW 강렬한 쾌감에 막상인 깨지는 대로,%CALLNAME:MASTER%(은)는 뜨거운 욕망의 덩어리를 토해냈다…
					CASE 1
						CALL HPH_PRINT,@"（タマタマ、あがって…HPHイッちゃいそうなんですね…HPH）", "L"
						CALL HPH_PRINT,@"「ちゅぼっHPHぐちゅっHPHじゅりゅぷぷっHPHじゅぶぶぅ～っHPH」", "L"
						PRINTFORML 화는 한층 추잡한 소리를 내면서%CALLNAME:MASTER%의 페니스를 몰아세우기 시작했다.
						PRINTFORML 작은 머리가 격렬하게 전후에 돌아다닐 때마다, 은백색의 아름다운 머리카락이 반짝반짝 빛나 흔들거린다.
						CALL HPH_PRINT,@"（%CALLNAME:MASTER%さんのせーえきっHPHのみたいよぉっ…HPH）", "L"
						PRINTFORMW %CALLNAME:MASTER%의 아랫배로부터 치솟아 오는 단 저리고가 정점으로 달하면(자), 노장의 첨단으로부터 정액이 내뿜었다…
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「んんっHPHいい、ですよ…このまま、くちのなかに…HPH」", "L"
						PRINTFORML 화는 사정의 징조를 감지하면(자), 입술을 꽉 움츠려 페니스를 잡아당겼다.
						CALL HPH_PRINT,@"「んぶっ、ちゅるっ、じゅぷ、ちゅうううぅ～～～～っHPH」", "W"
					CASE 1
						CALL HPH_PRINT,@"（おちんちん膨らんできたぁっ…HPHでちゃいそうなんだ…）","L"
						PRINTFORMW 화는 사정 직전의 페니스를 조용히 질네, 와 추잡한 물소리를 세워%CALLNAME:MASTER%(을)를 절정에 이끌었다…
				ENDSELECT
			ENDIF
		;技術不足
		;ELSEIF ABL:혀技術 < 2
		
		;何度も射精した
		ELSEIF TCVAR:350 > 3 && RAND:2
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「…わ…HPHまた、硬くなって…HPH」","L"
					PRINTFORML 몇번이나 정령을 토해내 더 흘립 하는 페니스를 앞에 두고, 화는 감탄의 한숨을 흘렸다.
					CALL HPH_PRINT,@"「ふふ、%CALLNAME:MASTER%さん…まだまだ搾ってあげますから、もっと沢山ぴゅっぴゅしてくださいねHPH」","L"
					PRINTFORMW 화는 괘씸한 미소를 띄워%CALLNAME:MASTER%의 리근을 차분히 빨았다…
				CASE 1
					;ふたなりMASTERでここを踏んじゃった場合、「私玉無しふたなり派なんですが…」ってなる人がいたら申し訳ないっすね。
					PRINTFORML 「이렇게 많이…어디에 모읍니까？…여기, 입니까？」
					PRINTFORML 화는%CALLNAME:MASTER%의 음낭에 얼굴을 접근해 황홀로 한 표정으로 웅취를 들이 마시고 있다.
					PRINTFORML 니오이를 즐겨 끝냈는지, 와 크게 한숨을 쉬면(자), 사랑하도록(듯이) 상냥하고 령구에 키스를 했다.
					CALL HPH_PRINT,@"「ちゅ、ちゅっ…HPHほんとうに、働き者さんですね…HPH」","W"
			ENDSELECT
		;勃起しきってない
		ELSEIF BASE:MASTER:발기 < 1000
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「ふっ、ちゅるっ、れろれろっHPH」","L"
					PRINTFORML 「아…빠르고, 움켜 떠 주세요♪」
					PRINTFORMW 아직 발기해 버리지 않은 페니스를 날름날름빨면서, 화는 싱긋 웃어 보였다.
				CASE 1
					CALL HPH_PRINT,@"「ふふ、全部咥えてあげちゃいます…あむぅ…HPH」","L"
					PRINTFORML 화는 부드러워지고 있는 페니스를 입 가득 뺨 펴면(자), 입안에서 와 자극하기 시작했다.
					CALL HPH_PRINT,@"「んっ、ぷ…HPHぐちゅ、ちゅる、ぷぁっ…おっきくなってきた…HPH」","L"
					PRINTFORMW 딱딱함을 늘려 용감하게 휘고는 가는 육 간을 응시해 화는 만족인 한숨을 흘렸다.
			ENDSELECT
		;精液中毒高め
		ELSEIF ABL:정액중독 > 3 && RAND:2
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「んむっ、ちゅぶっHPHじゅぼっHPHにゅりゅぷHPHちゅるるぅっHPH」","L"
					PRINTFORML 화는 눈을 난들과 빛낼 수 있는이면서, 맛있을 것 같게 페니스를 베어 물고 있다…
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%さんのニオイ、すっごく濃くて…頭がくらくらしちゃいます…HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「ん、ちゅっ、ぬりゅっ、ちゅうぅっHPH…%CALLNAME:MASTER%さんのかうぱー、おいし…HPH」","L"
					CALL HPH_PRINT,@"「もっと、ください…んっ、むちゅっ…りゅぷっ、ちゅりゅぅぅ…HPH」","L"
					PRINTFORMW 화는%CALLNAME:MASTER%의 페니스를 입술로 잡아당겨 흘러넘쳐 오는 앞달리기를 진하고 진하고와 군침이 돌아 삼킴하고 있다…
				CASE 2
					PRINTFORML （카치카치의 자지를 빨고 있을 뿐으로, 보지 실룩실룩 해 와버린다…）
					PRINTFORMW （%CALLNAME:MASTER%씨를 갖고 싶다…）
			ENDSELECT
		;通常
		ELSE
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「ぬちゅ、りゅぷ、ちゅるるっHPHにゅぽ、ぺろぺろぉっHPH」","L"
					CALL HPH_PRINT,@"「あは、%CALLNAME:MASTER%さん、腰浮いちゃってます…HPHそんなに、気持ちいいんですか？」","W"
				CASE 1
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%さんのニオイ、すごくて…私、興奮しちゃいます…HPH」","L"
					CALL HPH_PRINT,@"「ふぅっ、んぅ…ちゅぴ、ちゅぱ、れぷっ、れるぅぅ～っHPH」","L"
					PRINTFORMW 화는 차분히 페니스에 혀를 기게 한이면서, 갖고 싶어하는 듯하게 아랫배를 더듬고 있다…
			ENDSELECT
		ENDIF
		RETURN 1
	;非発情
	ELSE
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			;慣れてない
			IF EXP:정음경험 < 20
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「네？그리고 그렇게…？」
						PRINTFORMW 「원, 알았습니다…！이대로, 입속에…！」
					CASE 1
						PRINTFORML 「, , 구…」
						PRINTFORMW （아, 자지, 벌벌은…！？）
				ENDSELECT
			;精液中毒高め
			ELSEIF ABL:정액중독 > 3 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML （아, 자지, 벌벌 해…나와버릴 그렇구나…）
						PRINTFORML 「, 우우～～…」
						PRINTFORML 사정의 전조를 감지한 화는, 거의 무의식 중에 페니스를 후오에 념 글자 붐비었다.
						PRINTFORML 비강을 채우는 모의 수상한 웃음을 띄우면서, 와 소리를 내 달라붙어%CALLNAME:MASTER%(을)를 추적해 간다.
						CALL HPH_PRINT,@"（%CALLNAME:MASTER%さん、すごい顔しちゃってる…そんなに、気持ちいいんだ…HPH）","L"
						PRINTFORMW %CALLNAME:MASTER%하지만 무심코 소리를 흘리면(자), 화는 기쁜듯이 미소지어 한층 더 입봉사의 페이스를 빨리 했다…
					CASE 1
						PRINTFORML 「응, 나와버릴 것 같네요…」
						PRINTFORML 「좋아요, 이대로, 나썩어에 많이 주세요………」
						PRINTFORML 화는 사정 직전의 페니스를 입안에 맞아들여 추잡한 소리를 내 달라붙어서 놓지 않기 시작했다…
						CALL HPH_PRINT,@"「んぼっ、くぷっ、がぽっ、じゅるる、ふーっ…んぶっ、じゅ、じゅるりゅぷぅっHPH」","L"
						PRINTFORMW 스파트를 걸치도록(듯이) 화살 이어 빠른 자극이 주어져%CALLNAME:MASTER%(은)는 시원스럽게 정점을 지극히 끝냈다.
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「네？그리고 그렇게…？」
						PRINTFORML 「알았습니다…이대로, 두는 치노안으로…」
						PRINTFORMW 화는 첨단을 덥석 질와 혀끝에서 대통을 무돌려,%CALLNAME:MASTER%에 사정을 재촉했다…
					CASE 1
						PRINTFORML 「, , , 응응…이 나무 그렇습니까？」
						PRINTFORML 「괜찮아, 이대로 말해 주세요…」
						PRINTFORMW 「인내는, 하지 않아 좋으니까？」
				ENDSELECT
			ENDIF
		;技術不足
		;ELSEIF ABL:혀技術 < 2
		
		;何度も射精した
		ELSEIF TCVAR:350 > 3 && RAND:2
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「응응…또, 딱딱해져…」
					PRINTFORML 몇번이나 정령을 토해내 더 흘립 하는 페니스를 앞에 두고, 화는 감탄의 한숨을 흘렸다.
					CALL HPH_PRINT,@"「あー、むっ…もっほ、ひもひよふひへあげまふ…HPH」","L"
					PRINTFORMW 화는 페니스를 질네 붐비면(자), 닉코리와 미소지어 입봉사를 시작했다…
				CASE 1
					PRINTFORML 「굉장합니다,%CALLNAME:MASTER%씨…아직, 이렇게…」
					PRINTFORMW 화는 감복해 버린 모습으로%CALLNAME:MASTER%의 령구에 입 맞춤 하고 있다…
			ENDSELECT
		;勃起しきってない
		ELSEIF BASE:MASTER:발기 < 1000
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「아, 아직 부드럽다…」
					PRINTFORML 「네에에, 움켜 떠 주네요…,…」
					PRINTFORMW 아직 발기해 버리지 않은 페니스를 날름날름빨면서, 화는 싱긋 웃어 보였다.
				CASE 1
					PRINTFORML 「짜는 , 응, , 구…」
					PRINTFORML 화는 작은 채의 페니스를 입에 넣으면(자), 눈깔사탕을 빨고 굴리도록(듯이) 자극하기 시작했다.
					PRINTFORML 따뜻한 구강에 이끌린 귀두가 벌벌뛰어 주위면서, 점차 딱딱하게 부풀어 올라 간다…
					PRINTFORMW 「, , ,…(듣)묻지 말라고 왔습니다…」
			ENDSELECT
		;精液中毒高め
		ELSEIF ABL:정액중독 > 3 && RAND:2
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML （앞로부터, 장국이 나와…몹시, 인 맛…）
					PRINTFORML 화는 차례차례로 넘쳐 오는 앞달리기를 삼키면서 숨을 거칠게 하고 있다…
					PRINTFORML 「, , 진하고 응…♪, , 우우～～～」
					PRINTFORMW （자지, 맛있다…）
				CASE 1
					PRINTFORML 「, ,…아……대단히 뜨겁고, 딱딱하다…」
					PRINTFORML 화는 구강 성교에 열중해, 불쾌하게 허리를 구군요들 등무늬 페니스에 혀를 기게 하고 있다.
					PRINTFORML …
					PRINTFORMW 「, 응응…, , ,～～」
			ENDSELECT
		;通常
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「, ,…」
					PRINTFORMW 화는%CALLNAME:MASTER%의 표정을 치뜬 눈보람에 방문하면서, 열심히 봉사를 계속하고 있다…
				CASE 1
					PRINTFORML 「, ,…후~…」
					IF ABL:혀 < 2
						PRINTFORMW 「턱이 피로있네요…」
					ELSE
						PRINTFORMW 「 좀 더, 해 주네요…, ,…」
					ENDIF
				CASE 2
					PRINTFORML 「, , , 날름날름…」
					PRINTFORMW 「어떻게, 입니까…기분이 좋습니까？」
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

