;==================================================
;310,尻を撫でる
;TFLAG:193(1=不快(TALENT:臆病なら涙目) 2&&3=恥ずかしがる(TALENT:男嫌い兼MASTERが男なら、身をすくめる) 4=されるがまま)
;CFLAG:우후후==2&&TCVAR:20(パンツ無し顔面騎乗(70=クンニ強制 71=フェラチオ強制 72=アナル奉仕) 35=スカート内見せつけ 52=素股)
;CFLAG:우후후==2&&TEQUIP:식스나인(パンツありTCVAR:20(70～72)はこちら)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K65_310
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:수면
	ELSE
		IF TCVAR:발정
			IF CFLAG:기정사실 & 合意_うふふ
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「, 이제(벌써)…」
						PRINTFORMW 화는 욕정에 눈물을 글썽이면서 이쪽을 응시하고 있다…
					CASE 1
						PRINTFORML 「후~, , 응응…」
						PRINTFORMW 좀 작은 엉덩이가 애무되어 화는 뜨거운 한숨을 흘리고 있다…
					CASE 2
						CALL HPH_PRINT,@"「くぅ、ん…%CALLNAME:MASTER%さん…HPH」", "L"
						PRINTFORMW 화는%CALLNAME:MASTER%(으)로 하고 눈사태 걸려, 신체를 칠하고 있다…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「지금은 안돼, 안됩니다…」
						PRINTFORMW 화는 눈에 눈물을 배이게 해 오로지%CALLNAME:MASTER%의 손가락에 참고 있다…
					CASE 1
						PRINTFORML 「…！」
						PRINTFORMW 발정한 화는, 좀 작은 엉덩이를 겨 도는 쾌감에 견딜 수 있지 못하고 소리를 높여 버렸다…
				ENDSELECT
			ENDIF
		;膝枕直後
		ELSEIF PREVCOM == 305
			PRINTFORMW 「…！그러한 못된 장난한다면, 이제(벌써) 무릎 베개 해 주지 않아요…？」
		ELSEIF TALENT:연모 && RAND:4 > 0
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「, 갑자기 뭐 한다」
					PRINTFORML 「어…있고, 싫지 않지만…(은)는, , 응응…이제(벌써)……」
					PRINTFORMW %CALLNAME:MASTER%(은)는 기분이 끝날 때까지 부드러운 화의 고육을 즐겼다.
				CASE 1
					PRINTFORML 「후~, , 응응…」
					PRINTFORMW 좀 작은 엉덩이가 애무되어 화는 뜨거운 한숨을 흘리고 있다…
				CASE 2
					PRINTFORML 「그러한 못된 장난은 그만두어 주세요…」
					PRINTFORML 화는 부끄러운 듯이  반짝반짝 주위를 둘러보았지만,
					PRINTFORMW %CALLNAME:MASTER%의 애무를 거절할 것은 없었다…
			ENDSELECT
		ELSE
			SELECTCASE TFLAG:193
				;不快
				CASE 1
					PRINTFORML 「그만두어 주세요！」
					PRINTFORMW 화는%CALLNAME:MASTER%의 손을 먼지떨이 떨어뜨렸다.
				;恥ずかしがる
				CASE 2,3
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「저, 그…」
						CASE 1
							PRINTFORMW 「은, 부끄러우니까…」
					ENDSELECT
				;されるがまま
				CASE 4
					PRINTFORML 「구,…」
					PRINTFORMW 화는 조금몸을 비꼼, 허가를 청하도록(듯이)%CALLNAME:MASTER%(을)를 응시했다…
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
;==================================================
;311,抱き付く
;TFLAG:193(1=大成功 0=成功 -1=失敗)
;CFLAG:우후후==2&&TCVAR:20(32=前から抱きつく 60=ディープキス 73=ぱふぱふ 74=乳合わせ 77=乳を吸わせる ＶＡ性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K65_311
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_311_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CHK_DATENOW(CFLAG:LOCAL:데이트중)
	ELSEIF CFLAG:수면
	ELSE
		IF CFLAG:우후후 == 2
			SELECTCASE TCVAR:20
				;ディープキス
				CASE 60
					PRINTFORML 혀끼리를 얽히게 할 수 있는 만나면서 훨씬 꼭 껴안아 주면(자), 화는 기쁜듯이 꼭 껴안고 돌려주어 왔다…
					CALL HPH_PRINT,@"「んふ、ちゅ、ちゅる、ちゅむ、ちゅぱ、ぷぁ……あの、もう一回…、ん、ちゅうう…HPH」", "W"
				;だいしゅきホールド
				CASE 86
					CALL HPH_PRINT,@"「っHPHあっHPHもっとっHPHもっとぉっHPH」", "L"
					PRINTFORMW 몸집이 작은 신체를 꼭 껴안아 좁은 질내를 찔러 돌리면(자), 화는 감극한 것 같은 절규를 질러 스스로 허리를 꽉 눌러 왔다…
				CASEELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「후~…%CALLNAME:MASTER%씨, 따뜻하다…」
						CASE 1
							PRINTFORMW 「 좀 더 강하고, 꼭 껴안아 주세요…」
					ENDSELECT
					
			ENDSELECT
		ELSEIF TALENT:연인 && RAND:2
			IF TCVAR:발정
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI("控えめ",3)%…あの、%CALLNAME:MASTER%さん…HPH」","L"
						PRINTFORMW %CALLNAME:MASTER%에 꼭 껴안을 수 있던 채로, 화는 머뭇머뭇내퇴를 조정이라고 있다…
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%…はぁっ…HPH」","L"
						PRINTFORMW 화는%CALLNAME:MASTER%의 키를 히 쉿안으면서, 부드러운 신체를 칠해 온다…
						SIF RAND:3 == 0
							PRINTFORMW 그녀의 냄새를 남기 듯이 열렬한 포옹은, 마치 마킹과 같다…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「ん…HPH」","L"
						PRINTFORMW 「쭉, 이렇게 해 아픕니다…」
					CASE 1
						PRINTFORML 「응…♪」
						CALL HPH_PRINT,@"「抱きしめてください…もっと、つよく…HPH」","W"
				ENDSELECT
			ENDIF
		ELSEIF TALENT:연모
			IF TCVAR:발정
				IF BASE:MASTER:발기 > 999 && CFLAG:기정사실 > 0 && RAND:2 == 0
					PRINTFORML 「아는,%CALLNAME:MASTER%씨의…이렇게 되어 버린다」
					CALL HPH_PRINT,@"「ね、どこか…人目のないところまでいきませんか…HPH」", "W"
				ELSE
					PRINTFORML （이렇게 해 꼭 껴안을 수 있는 것만으로, 배의 안쪽응, 응은 해버린다…）
					PRINTFORMW （…%CALLNAME:MASTER%씨를 갖고 싶어서, 체내가 쑤셔 버린다 다…）
				ENDIF
			ELSE
				IF BASE:MASTER:발기 > 999 && CFLAG:기정사실 > 0 && RAND:2 == 0
					PRINTFORMW 「저,%CALLNAME:MASTER%씨…맞아,…」
					SIF PALAM:욕정 >= PALAMLV:4
						PRINTFORMW （…나도 이상한 기분이 되어 와 버렸다…）
				ELSE
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ん…HPH」", "L"
							PRINTFORMW 「%CALLNAME:MASTER%씨, 좋은 냄새 하네요…」
						CASE 1
							PRINTFORMW 「네에에,%CALLNAME:MASTER%씨, 따뜻하다…」
						CASE 2
							PRINTFORML 「%CALLNAME:MASTER%씨…」
							PRINTFORMW 화는%CALLNAME:MASTER%의 가슴에 머리를 맡기고 있다…
					ENDSELECT
				ENDIF

			ENDIF
		ELSEIF TALENT:사모
			IF TCVAR:발정
				PRINTFORML 「아, 그…」
				PRINTFORMW 화는 머뭇머뭇내퇴를 조정이라고 있다…
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORML 「우우…」
						PRINTFORMW 화는 뺨을 붉혀 되는 대로 되어 있다.
					CASE 1
						PRINTFORMW 화는 흠칫흠칫%CALLNAME:MASTER%의 등에 팔을 돌려, 꼭 껴안고 돌려주어 왔다…
				ENDSELECT
			ENDIF
		;・非陥落時
		;親密>5 突然の事に驚いたのかじたばたと身体を揺らしている…
		ELSEIF ABL:친밀 > 5
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「있고, 갑자기 어떻게 합니까…？」
					PRINTFORMW 갑자기 달라붙어져 화는 곤혹하고 있다.
				CASE 1
					PRINTFORML 「우우…」
					PRINTFORMW 화는 반응이 곤란하고 있는 모습이지만, 피하려는 의지는 없는 것 같다.
			ENDSELECT
		;親密<=5 大声を上げて%CALLNAME:MASTER%を突き飛ばした
		ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「꺄아！？, 놀래키지 말아 주세요」
			CASE 1
				PRINTFORML 「꺄아！？」
				PRINTFORML 돌연 배후로부터 달라붙어져 놀라 날뛰 화의 팔꿈치가%CALLNAME:MASTER%만 떨어져에 클린 히트 했다…
				PRINTFORMW 「이다, 괜찮습니까！？」
		ENDSELECT
		ENDIF
	ENDIF

ENDIF
RETURN 1
;==================================================
;312,キスする
;TFLAG:193(99=拒絶(キス不可) 1=大成功 0=成功 -1=失敗)
;CFLAG:우후후==2&&TCVAR:20(32=前から抱きつく 50=手淫 60=キス 性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;PREVCOM(305=膝枕
;TFLAG:400(1=デート先or地底のモブが反応する)
;==================================================
@M_KOJO_MESSAGE_COM_K65_312
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_312_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70	
	ELSEIF CFLAG:수면
	ELSEIF CFLAG:TARGET:동침중
		PRINTFORML 「응…」
		CALL HPH_PRINT,@"「えへへ…HPH」", "W"
	;拒絶
	ELSEIF TFLAG:193 == 99
		PRINTFORML 「, 가까워요…？」
		PRINTFORMW 화는 몸을 당겨, 의심에%CALLNAME:MASTER%(을)를 응시하고 있다…
	ELSE
		;初めて
		IF FIRSTTIME(SELECTCOM)
			;デートキスイベント済み
			IF EXP:TARGET:키스경험 > 1
				PRINTFORML 「응…」
				PRINTFORML 「…%CALLNAME:MASTER%씨로부터 키스 해 주는 것은, 이것이 처음이네요」
				PRINTFORMW 「네에에, 어쩐지 기쁩니다」
			ELSE
				PRINTFORML 「응…！？」
				PRINTFORML 「, 갑자기 무엇을…！？」
				PRINTFORML （아, 이것은, 퍼스트 키스…？）
				PRINTFORMW 화는 보자리수 같은 표정으로 자신의 입술에 닿고 있다.
			ENDIF
		ELSEIF CFLAG:우후후 == 2
			SELECTCASE TCVAR:20
				;だいしゅきホールド
				CASE 86
					CALL HPH_PRINT,@"「んっ！？んふぅっ…んちゅ、ちゅうううっHPH」","L"
					PRINTFORML 꽉 안아 도착하는 그녀에게 응해%CALLNAME:MASTER%하지만 입맞춤을 하면(자),
					PRINTFORMW 화는 감극한 것처럼 숨을 삼켜, 입술에 달라붙어 왔다…
				;キス
				CASE 60
					CALL HPH_PRINT,@"「んんっ、む、ふふ…ちゅ、ちゅうっHPHぺろっ、れるれるぅ…HPH」", "L"
					PRINTFORMW %CALLNAME:MASTER%하지만 키스에 응하면(자), 화는 한층 더 열렬하게 혀를 걸어 맞추었다…
				CASEELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「, , ,…」
							PRINTFORMW 화는 열중해%CALLNAME:MASTER%(와)과 입맞춤하고 하고 있다.
						CASE 1
							CALL HPH_PRINT,@"「んむ、ぷぁ…キスって、すっごく気持ちいいですね…HPHちゅ、ちゅぷ、ちゅむぅ…」", "W"
					ENDSELECT
			ENDSELECT
		ELSEIF TCVAR:발정
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「はぁ、%CALLNAME:MASTER%さんっ、ちゅ、ちゅうぅ…HPH」", "L"
					PRINTFORMW 화는 적극적으로 혀를 걸어, 키스의 유열에 잠겨 있다…
				CASE 1
					CALL HPH_PRINT,@"「ちゅる、んちゅ、ちゅ、れるぅ…HPH」", "L"
					PRINTFORMW （%CALLNAME:MASTER%씨와의 키스, 몹시 기분이 좋다…）
					SIF CFLAG:기정사실 & 合意_うふふ
						PRINTFORMW （하지만, 참을 수 없게 되어버릴 그렇게…）
			ENDSELECT
		ELSEIF ABL:친밀 >= 5
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「,…」
					PRINTFORM 화는
					SIF TALENT:MASTER:체형 >= 0
						PRINTFORM 발끝 서가 되어
					PRINTFORMW %CALLNAME:MASTER%(와)과의 키스를 즐기고 있다…
				CASE 1
					PRINTFORML 「…네에에…」
					PRINTFORMW 화는 희미하게 뺨을 붉혀 미소지었다.
			ENDSELECT
		ELSE
			PRINTFORMW 「…！？…」
			PRINTFORMW 「어와 그…」
		ENDIF
	ENDIF

ENDIF
RETURN 1
;==================================================
;313,胸愛撫
;CFLAG:우후후==2&&TCVAR:20(27=谷間を見せる 33=胸を揉ませる 53=パイズリ(小分岐は地の文EVENT_MESSAGE_COM_セクハラ.ERB参照) 73=ぱふぱふ 77=乳を吸わせる 性交(85=Ｖ挿入する 87=Ａ挿入する 88=腰振りＶ 89=腰振りＡ) 86=だいしゅきホールド)
;ABL:B감각(2未満=性的には感じてない 5未満=多少感じてる 5以上=カイカン)
;TFLAG:400(1=デート先or地底のモブが反応する)
;乳のサイズでも変わる場合があるので、地の文EVENT_MESSAGE_COM_セクハラ.ERB参照
;台詞＋胸サイズごとのフレーバーテキストって形式が無難かしらね
;==================================================
@M_KOJO_MESSAGE_COM_K65_313
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K65_313_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_313_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	
	ELSEIF CFLAG:수면
	
	ELSEIF CFLAG:우후후 == 2
		IF TCVAR:발정
			SELECTCASE TCVAR:20
				;胸を揉ませる
				CASE 33
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「ふっ、ぅ…あはっHPH」", "L"
						PRINTFORMW 화는 눈을 난들과 시키면서, 당신의 손을 보다 강하게 가슴으로 꽉 눌렀다.
					CASE 1
					;きょにゅー以上
						IF TALENT:가슴사이즈 > 0
							PRINTFORML 「네에, 가슴에는 상당히 자신 있습니다…」
							PRINTFORML 가슴 팍에 꽉 눌렀다%CALLNAME:MASTER%의 손바닥마다 스스로의 풍유를 비비어 하기 만여,
							PRINTFORMW 화는 유열의 표정을 띄우고 있다…
						;ひんにゅー以下
						ELSEIF TALENT:가슴사이즈 < 0
							CALL HPH_PRINT,@"「はぁ…っHPHお、おっぱい、気持ちいい…ですっHPH」", "L"
							PRINTFORMW 욕정에 날카로워진 유두를 누르도록(듯이), 화는%CALLNAME:MASTER%의 손바닥을 가슴 팍에 꽉 누른다…
						;ふにゅー
						ELSE
							CALL HPH_PRINT,@"「%CALLNAME:MASTER%さん、もっと、つよく…っHPHHPH」", "L"
							PRINTFORMW 그녀의 요구에 응해보다 강하게 가슴을 비비어 주면(자), 화는 기쁜듯이 몸을 진동시켰다.
						ENDIF
					CASE 3
						CALL HPH_PRINT,@"「はっぁHPH乳首っ、ちくびぃぃっHPH」", "L"
						PRINTFORMW 긴장된 유두를 손톱의 끝으로 상냥하고 칼리 칼리와 히쓰여되어 화는 교성을 주어 쾌락에 잠겨 있다…
				ENDSELECT
				;パイズリ
				CASE 53
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ひゃうん！っはぁ、もっとぉ…っHPH」", "L"
							PRINTFORMW %CALLNAME:MASTER%에 유방을 희롱해져 화는 뇌끊임없는 한숨을 흘리면서도 젖봉사를 계속하고 있다.
						CASE 1
							CALL HPH_PRINT,@"「あはっ…HPHおっぱいに、%CALLNAME:MASTER%さんのにおい、つけてください…HPH」", "W"
						CASE 2
							PRINTFORML 「, 이제(벌써), 지금은 내가%CALLNAME:MASTER%씨를 탓합니까들…」
							CALL HPH_PRINT,@"「あんまり、やんっHPHいたずらしないでください…」", "W"
					ENDSELECT
				;乳を吸わせる
				CASE 77
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「ふぁんっHPHそんなふうに乳首ちゅぅって吸われると、お腹の奥がきゅんってしちゃいます…HPH」", "W"
						CASE 1
