;40,ローター
;==================================================
@M_KOJO_MESSAGE_COM_K124_40
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K124_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K124_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:장난 == 2
		PRINTFORML 「…구, 우우…」
		PRINTFORML 오키나는 잘 수 없는 듯이  하고 있다….
		RETURN 1
	ELSEIF TCVAR:124:만취 == 1
		RETURN 1
	;恋慕or恋人
	ELSEIF TALENT:124:연인 || TALENT:124:연모 || TALENT:124:애욕
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「개, 이런 완구로……, 으응%UNICODE(0x2764)%」
			CASEELSE
				PRINTFORMW 「우우, 와, 기분이 좋은데……어쩐지 석연치 않다…！」
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「구, 응아！, 두고！언제까지 꽉 누르고 있는 작정(생각)이다……！」
			CASEELSE
				PRINTFORMW 「히우우, 나 , 해, 끈질겨！이제(벌써) 그만두어라……！」
		ENDSELECT
	ENDIF
ENDIF

RETURN 1
